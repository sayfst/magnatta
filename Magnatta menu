                                    if background.BackgroundColor3 == Color3.fromRGB(255, 0, 0) then
                                        background.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                                        Tab.Visible = true
                                    else
                                        background.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
                                        Tab.Visible = false
                                    end
                                end)
                        
