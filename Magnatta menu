                                local BOTOES = {}
                        
                                function BOTOES:CreateButton(title, callback)
                                    callback = callback or function()end
                        
                                    local BotaoNormal = Instance.new("TextButton")
                        
                                    BotaoNormal.Name = "BotaoNormal"
                                    BotaoNormal.Parent = Items
                                    BotaoNormal.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    BotaoNormal.BackgroundTransparency = 1.000
                                    BotaoNormal.BorderSizePixel = 0
                                    BotaoNormal.Text = title
                                    BotaoNormal.Size = UDim2.new(0, 186, 0, 19)
                                    BotaoNormal.Font = Enum.Font.SourceSans
                                    BotaoNormal.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    BotaoNormal.TextSize = 14.000
                                    BotaoNormal.MouseButton1Click:Connect(function()
                                        pcall(callback)
                                    end)
                                end
                        
                                function BOTOES:CreateToggle(toggleTigle, callback)
                                    local Enabled = false
                                    
                                    local ToggleNormal = Instance.new("TextButton")
                                    local backgroundd = Instance.new("Frame")
                        
                                    ToggleNormal.Name = "ToggleNormal"
                                    ToggleNormal.Parent = Items
                                    ToggleNormal.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    ToggleNormal.BackgroundTransparency = 1.000
                                    ToggleNormal.BorderSizePixel = 0
                                    ToggleNormal.Size = UDim2.new(0, 186, 0, 19)
                                    ToggleNormal.Font = Enum.Font.SourceSans
                                    ToggleNormal.Text = toggleTigle
                                    ToggleNormal.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    ToggleNormal.TextSize = 14.000
                                    ToggleNormal.MouseButton1Click:Connect(function()
                                        if backgroundd.BackgroundColor3 == Color3.fromRGB(16, 134, 244) then
                                            backgroundd.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
                                        else
                                            backgroundd.BackgroundColor3 = Color3.fromRGB(16, 134, 244)
                                        end
                                        task.wait()
                                        Enabled = not Enabled
                                        pcall(callback, Enabled)
                                    end)
                        
                                    backgroundd.Name = "background"
                                    backgroundd.Parent = ToggleNormal
                                    backgroundd.BackgroundColor3 = Color3.fromRGB(16, 134, 244)
                                    backgroundd.BorderSizePixel = 0
                                    backgroundd.Position = UDim2.new(0.036842104, 0, 0.210526317, 0)
                                    backgroundd.Size = UDim2.new(0, 10, 0, 10)
                                end
                        
                                function BOTOES:CreateTextBox(textBoxTitle,valor, callback)
                                    callback = callback or function() print('No function') end
                                    valor = valor or 'String'
                                    local TextBoxNormal = Instance.new("TextBox")
                        
                        
                                    TextBoxNormal.Name = "TextBoxNormal"
                                    TextBoxNormal.Parent = Items
                                    TextBoxNormal.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    TextBoxNormal.BackgroundTransparency = 1.000
                                    TextBoxNormal.BorderSizePixel = 0
                                    TextBoxNormal.Text = textBoxTitle
                                    TextBoxNormal.Size = UDim2.new(0, 186, 0, 19)
                                    TextBoxNormal.Font = Enum.Font.SourceSans
                                    TextBoxNormal.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TextBoxNormal.TextSize = 14.000
                                    TextBoxNormal.FocusLost:Connect(function(a)
                                        if a then
                                            if valor == 'Number' then
                                                pcall(callback, tonumber(TextBoxNormal.Text))
                                            elseif valor == 'String' then
                                                pcall(callback, tostring(TextBoxNormal.Text))
                                            end
                                            task.wait()
                                            TextBoxNormal.Text = textBoxTitle
                                        end
                                    end)
                                end
                        
                                function BOTOES:CreateSlider(sliderText,minvaluee,maxvaluee,callback, defvalue, bool)
                                    defvalue = defvalue or 0
                                    minvaluee = minvaluee or 0
                                    maxvaluee = maxvaluee or 100
                                    callback = callback or function()end
                        
                                    if bool == true then
                                        pcall(callback, tonumber(defvalue))
                                    end
                                    local SliderNormal = Instance.new("TextLabel")
                                    local ValorSlide = Instance.new("TextBox")
                                    local Botao = Instance.new("TextButton")
                                    local TextLabel = Instance.new("TextLabel")
                        
                                    SliderNormal.Name = "SliderNormal"
                                    SliderNormal.Parent = Items
                                    SliderNormal.Active = true
                                    SliderNormal.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    SliderNormal.BackgroundTransparency = 1.000
                                    SliderNormal.BorderSizePixel = 0
                                    SliderNormal.Selectable = true
                                    SliderNormal.Size = UDim2.new(0, 186, 0, 19)
                                    SliderNormal.Font = Enum.Font.SourceSans
                                    SliderNormal.Text = sliderText
                                    SliderNormal.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    SliderNormal.TextSize = 13.000
                                    SliderNormal.TextYAlignment = Enum.TextYAlignment.Top
                        
                                    ValorSlide.Name = "ValorSlide"
                                    ValorSlide.Parent = SliderNormal
                                    ValorSlide.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                                    ValorSlide.BackgroundTransparency = 0
                                    ValorSlide.Position = UDim2.new(0.753000021, 0, 0, -6)
                                    ValorSlide.Size = UDim2.new(0, 8, 0, 8)
                                    ValorSlide.Font = Enum.Font.SourceSans
                                    ValorSlide.Text = defvalue
                                    ValorSlide.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    ValorSlide.TextSize = 14.000
                                    ValorSlide.Active = false
                                    local frame = TextLabel
                                    ValorSlide.FocusLost:Connect(function()
                                        bool = ValorSlide.Text
                                        bool = tonumber(bool) or 0
                                        pcall(callback(bool))
                                        
                                    end)
                        
                                    Botao.Name = "Botao"
                                    Botao.Parent = SliderNormal
                                    Botao.BackgroundColor3 = Color3.fromRGB(54, 54, 54)
                                    Botao.BackgroundTransparency = 0.050
                                    Botao.BorderSizePixel = 0
                                    Botao.Position = UDim2.new(0.0894736871, 0, 0.684210539, 0)
                                    Botao.Size = UDim2.new(0, 160, 0, 9)
                                    Botao.Font = Enum.Font.SourceSans
                                    Botao.Text = ""
                                    Botao.TextColor3 = Color3.fromRGB(0, 0, 0)
                                    Botao.TextSize = 14.000
                        
                                    TextLabel.Parent = Botao
                                    TextLabel.BackgroundColor3 = Color3.fromRGB(17, 136, 254)
                                    TextLabel.BorderSizePixel = 0
                                    TextLabel.Size = UDim2.new(0, 0, 0, 9)
                                    TextLabel.Font = Enum.Font.SourceSans
                                    TextLabel.Text = ""
                                    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
                                    TextLabel.TextSize = 14.000
                        
                                    local button = Botao
                                
                                    
                                        
                                    local TextLabel_2 = ValorSlide
                                        
                                    local maxvalue = maxvaluee
                                    local minvalue = minvaluee
                                    local mouse = game.Players.LocalPlayer:GetMouse()
                                    local uis = game:GetService('UserInputService')
                                    button.MouseButton1Down:Connect(function()
                                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 160) * frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                                        pcall(function()
                                            callback(Value)            
                                        end)
                                        
                                        moveconnection = mouse.Move:Connect(function()
                                            TextLabel_2.Text = Value
                                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 160) * frame.AbsoluteSize.X) + tonumber(minvalue))
                                            pcall(function()
                                                callback(Value)
                                            end)
                                            frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, 160), 0, 9)
                                            pos = SliderNormal.Position
                                        end)
                                        releaseconnection = uis.InputEnded:Connect(function(Mouse)
                                            if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                                                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 160) * frame.AbsoluteSize.X) + tonumber(minvalue))
                                                pcall(function()
                                                    callback(Value)
                                                end)
                                                frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, 160), 0, 9)
                                                moveconnection:Disconnect()
                                                releaseconnection:Disconnect()
                                            end
                                        end)
                                    end)
                                end
                        
                        
                        
                                return BOTOES
                            end
                        
                            return Tabss
                        end
                    end
                    getgenv().ExecCmd = {}
                    getgenv().Toggle = {}
                    function ExecCmd(cmd)
                        for i,v in pairs(getgenv().ExecCmd) do
                            if v.tt == cmd then
                                pcall(function()
                                    v.ff()
                                end)
                            end
                        end
                    end
                    function ToggleToggle(cmd)
                        for i,v in pairs(getgenv().Toggle) do
                            if v.tt == cmd then
                                pcall(function()
                                    v.ff()
                                end)
                            end
                        end
                    end
                    local speaker = game.Players.LocalPlayer
                    local Menu = magnata:CreateWindow('Magnatta', {
                        --[[Under line Color]] Color3.fromRGB(127, 0, 156);
                        --[[Normal buttons Color]] Color3.fromRGB(127, 0, 156);
                        --[[Normal TextLabel Color]] Color3.fromRGB(75, 0, 92);
                        --[[TextBox Text, Normal is: (Type here)]] '(Insira um Valor)';
                    })

                    local rr = {

                    }
                    function rainbow(item, duration, nome)
                        local color = item
                        local tempoRGB =  duration
                        rr[#rr + 1] = nome
                        task.spawn(function()
                            while true do
                                if table.find(rr, nome) then
                                    for i = 1,255 do
                                        color.BackgroundColor3 = Color3.fromRGB(255, i, 0)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color.BackgroundColor3 = Color3.fromRGB(i, 255, 0)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 1, 255 do
                                        color.BackgroundColor3 = Color3.fromRGB(0, 255, i)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color.BackgroundColor3 = Color3.fromRGB(0, i, 255)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 1, 255 do
                                        color.BackgroundColor3 = Color3.fromRGB(i, 0, 255)
                                        task.wait(tempoRGB)
                                    end
                                    for i = 255,0, -1 do
                                        color.BackgroundColor3 = Color3.fromRGB(255, 0, i)
                                        task.wait(tempoRGB)
                                    end
                                end
                                task.wait()
                            end
                        end)                       
                    end
                    function unrainbow(item)
                        for i = 1,#rr do
                            if rr[i] == item then
                                table.remove(rr, i)
                            end
                        end
                    end

                    local TabEsp = Menu:CreateTab('Esp')
                    
                    function R15(speaker)
                        if not getHumanoid(speaker) then
                            return false
                        end
                        if speaker.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                            return true;
                        else
                            return false;
                        end
                    end
                    function FindAccessory(name)
                        if typeof(name) == 'string' then
                            if speaker.Character:FindFirstChild(name) then
                                return true;
                            end
                            return false
                        elseif typeof(name) == 'table' then
                            local On = false
                            for i,v in pairs(name) do
                                if speaker.Character:FindFirstChild(v) then
                                    On = true
                                else
                                    On = false 
                                end
                                
                            end
                            return On
                        end
                    end
                    function getCharacter(plr)
                        if plr.Character ~= nil then
                            return plr.Character
                        else
                            return false
                        end
                    end
                    function checkCharacter(plr)
                        if plr.Character ~= nil then 
                            if getRoot(plr) ~= false then
                                if plr.Character:FindFirstChild('Head') ~= nil then
                                    return true
                                end
                            else
                                return false
                            end
                        else
                            return false
                        end
                        return false
                    end
                    function getImage(image)
                        local IMAGENS = {
                            {
                                {'Sino', 'Bell'},
                                "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
                            },
                            {
                                {'Sino2', 'Bell2'},
                                "rbxthumb://type=Asset&id=5107182114&w=150&h=150"
                            }
                        }
    
                        for i,im in pairs(IMAGENS) do
                            
                            for i,v in pairs(im[1]) do
                                if image == v then
                                    return im[2]
                                end
                            end
                        end
                    end
                    local CanRefresh = true
                    function RefreshPrisonlife()
                        if CanRefresh then
                            CanRefresh = false
                            local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            local OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                            local camera = workspace.CurrentCamera
                            local oldCameraPos = camera.CFrame
                            spawn(function()
                                game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
                                repeat
                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(OldPos2 + Vector3.new(0, 2, 0))
                                    task.wait()
                                until (getRoot(speaker).Position - OldPos2).Magnitude <= 10
                                CanRefresh = true
                                return true
                            end)
                            workspace.Remote.loadchar:InvokeServer(game:GetService("Players").LocalPlayer)
                        end
                    end
                    function KillPrison(player)
                        if typeof(player) == 'Instance' then
                            if player.TeamColor == speaker.TeamColor then
                                workspace.Remote.TeamEvent:FireServer('Medium stone grey')
                                task.wait()
                            end
                            target = player
                            speaker = game.Players.LocalPlayer
                            if speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870') then else
                                local args = {
                                    [1] = workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP
                                }
                    
                                workspace.Remote.ItemHandler:InvokeServer(unpack(args))
                            end
                            
                            local Remington_870 = speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870')
                
                            Player = target
                            local FireEvent = {
                                [1] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [2] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [3] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [4] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [5] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [6] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [7] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }, [8] = {
                                    ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                    ["Distance"] = 0, 
                                    ["Cframe"] = CFrame.new(), 
                                    ["Hit"] = workspace[Player.Name].Head
                                }
                            }
        
                            
                            if Remington_870.Parent == speaker.Backpack then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Backpack[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Remington 870"))
                            elseif Remington_870.Parent == speaker.Character then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Character[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Remington 870"))
                            end
                            spawn(function()
                                wait(.1)
                                task.spawn(function()
                                    local explode = Instance.new('Explosion', workspace)
    
                                    explode.Position = player.Character.Head.Position
                                end)
                                CriminalP()
                            end)
                        
                        elseif typeof(player) == 'table' then
                            
                            target = player
                            speaker = game.Players.LocalPlayer
                            if speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870') then else
                                local args = {
                                    [1] = workspace.Prison_ITEMS.giver:FindFirstChild("Remington 870").ITEMPICKUP
                                }
                    
                                workspace.Remote.ItemHandler:InvokeServer(unpack(args))
                            end
                            
                            local Remington_870 = speaker.Backpack:FindFirstChild('Remington 870') or speaker.Character:FindFirstChild('Remington 870')
                
    
                            local FireEvent = {
                                
                            }
                            for i,v in pairs(player) do
                                if v.Character and v.Character:FindFirstChild('Head') ~= nil then
                                    for i = 1,8 do
                                        FireEvent[#FireEvent + 1] = {
                                            ["RayObject"] = Ray.new(Vector3.new(), Vector3.new()), 
                                            ["Distance"] = 0, 
                                            ["Cframe"] = CFrame.new(), 
                                            ["Hit"] = v.Character.Head
                                        }
                                    end
                                end
                            end
                            if Remington_870.Parent == speaker.Backpack then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Backpack[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Remington 870"))
                            elseif Remington_870.Parent == speaker.Character then
                                game:GetService("ReplicatedStorage").ShootEvent:FireServer(FireEvent, game.Players.LocalPlayer.Character[Remington_870.Name])
                                game:GetService("ReplicatedStorage").ReloadEvent:FireServer(game:GetService("Players").LocalPlayer.Character:FindFirstChild("Remington 870"))
                            end
                            spawn(function()
                                wait(.1)
                                task.spawn(function()
                                    for i,v in pairs(player) do
                                        local explode = Instance.new('Explosion', workspace)
    
                                        explode.Position = v.Character.Head.Position
                                    end
                                end)
                                CriminalP()
                            end)
                        end
                    end
                    function FindTool(toolName, plr)
                        plr = plr or game.Players.LocalPlayer
                        backpack = plr.Backpack
                        char = plr.Character
                    
                        if backpack:FindFirstChild(toolName) then
                            return backpack:FindFirstChild(toolName)
                        elseif char:FindFirstChild(toolName) then
                            return char:FindFirstChild(toolName) 
                        else
                            return false;                       
                        end
                    end
                    

                    function NotifyV2(title, text, duration)
                        local BlackGui = Instance.new("ScreenGui")
                        local Frame = Instance.new("Frame")
                        local TextLabel = Instance.new("TextLabel")
                        local TextLabel_2 = Instance.new("TextLabel")
                        
                        BlackGui.Name = "BlackGui"
                        BlackGui.Parent = game.CoreGui
                        BlackGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                        BlackGui.IgnoreGuiInset = true
                        
                        Frame.Parent = BlackGui
                        Frame.AnchorPoint = Vector2.new(0.5, 0.5)
                        Frame.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
                        Frame.BorderSizePixel = 0
                        Frame.Position = UDim2.new(0.5, 0, 0.5, 0)
                        Frame.Size = UDim2.new(1, 0, 1, 0)
                        
                        TextLabel.Parent = Frame
                        TextLabel.AnchorPoint = Vector2.new(0.5, 0)
                        TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel.BackgroundTransparency = 1.000
                        TextLabel.Position = UDim2.new(0.5, 0, 0.0759999976, 0)
                        TextLabel.Size = UDim2.new(0.535663366, 0, 0.150000006, 0)
                        TextLabel.Font = Enum.Font.SourceSansBold
                        TextLabel.Text = title
                        TextLabel.TextColor3 = Color3.fromRGB(225, 225, 225)
                        TextLabel.TextSize = 60.000
                        
                        TextLabel_2.Parent = Frame
                        TextLabel_2.AnchorPoint = Vector2.new(0.5, 0.5)
                        TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                        TextLabel_2.BackgroundTransparency = 1.000
                        TextLabel_2.Position = UDim2.new(0.5, 0, 0.575156569, 0)
                        TextLabel_2.Size = UDim2.new(0.535663366, 0, 0.150000006, 0)
                        TextLabel_2.Font = Enum.Font.SourceSansBold
                        TextLabel_2.Text = text
                        TextLabel_2.TextColor3 = Color3.fromRGB(225, 225, 225)
                        TextLabel_2.TextSize = 45.000
                        task.spawn(function()
                            for i,v in pairs(game:GetDescendants()) do
                                if v.ClassName == 'ScreenGui' and v ~= BlackGui then
                                    local oldEnabled = v.Enabled 
                                    v.Enabled = false
                                    task.spawn(function()
                                        wait(duration)
                                        v.Enabled = oldEnabled
                                    end)
                                end 
                            end
                            wait(duration)
                            BlackGui:Destroy()
                        end)
                    end
                    
                    function getHumanoid(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChildWhichIsA('Humanoid') then
                                return plr.Character:FindFirstChildWhichIsA('Humanoid');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    function getHead(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChild('Head') then
                                return plr.Character:FindFirstChild('Head');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    function getRoot(plr)
                        if plr.Character ~= nil then
                            if plr.Character:FindFirstChild('HumanoidRootPart') then
                                return plr.Character:FindFirstChild('HumanoidRootPart');
                            else
                                return false;
                            end
                        else
                            return false
                        end
                    end
                    
