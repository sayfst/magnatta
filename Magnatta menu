                                    TituloToggle.TextXAlignment = Enum.TextXAlignment.Left
                                end                    
                                function Items:CreateSlider(title, minvalue, maxvalue, callback, NumerosComPonto)
                                    NumerosComPonto = NumerosComPonto or false
    
                                    minvalue = minvalue or 1
                                    maxvalue = maxvalue or 100
                                    callback = callback or function()end
                                    defvalue = defvalue or 10
                        
                                    
                                    local Slider = Instance.new("TextLabel")
                                    local BotaoSlider = Instance.new("TextButton")
                                    local TextLabel = Instance.new("TextLabel")
                                    local TextLabel_2 = Instance.new("TextBox")
                                    local TituloSlider = Instance.new("TextLabel")
                                    
                                    Slider.Name = "Slider"
                                    Slider.Parent = TabLocal
                                    Slider.Active = true
                                    Slider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    Slider.BackgroundTransparency = 1.000
                                    Slider.BorderSizePixel = 0
                                    Slider.Position = UDim2.new(0, 0, 0.277777791, 0)
                                    Slider.Selectable = true
                                    Slider.Size = UDim2.new(0, 701, 0, 50)
                                    Slider.Font = Enum.Font.SourceSans
                                    Slider.Text = ""
                                    Slider.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    Slider.TextScaled = true
                                    Slider.TextSize = 14.000
                                    Slider.TextWrapped = true
                                    Slider.TextXAlignment = Enum.TextXAlignment.Left
                                    
                                    BotaoSlider.Name = "BotaoSlider"
                                    BotaoSlider.Parent = Slider
                                    BotaoSlider.Active = false
                                    BotaoSlider.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
                                    BotaoSlider.BorderSizePixel = 0
                                    BotaoSlider.Position = UDim2.new(0.022824537, 0, 0.560000002, 0)
                                    BotaoSlider.Selectable = false
                                    BotaoSlider.Size = UDim2.new(0, 629, 0, 11)
                                    BotaoSlider.Font = Enum.Font.SourceSans
                                    BotaoSlider.Text = ""
                                    BotaoSlider.TextColor3 = Color3.fromRGB(0, 0, 0)
                                    BotaoSlider.TextSize = 14.000
                                    
                                    TextLabel.Parent = BotaoSlider
                                    TextLabel.BackgroundColor3 = config[2]
                                    TextLabel.BorderSizePixel = 0
                                    TextLabel.Size = UDim2.new(0, 0, 1, 0)
                                    TextLabel.Font = Enum.Font.SourceSans
                                    TextLabel.Text = ""
                                    TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
                                    TextLabel.TextSize = 14.000
                                    
                                    TextLabel_2.Parent = Slider
                                    TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    TextLabel_2.BackgroundTransparency = 1.000
                                    TextLabel_2.Position = UDim2.new(0.941512108, 0, 0.159999996, 0)
                                    TextLabel_2.Size = UDim2.new(0, 31, 0, 31)
                                    TextLabel_2.Font = Enum.Font.SourceSans
                                    TextLabel_2.Text = "0"
                                    TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TextLabel_2.TextSize = 32.000
                                    
                                    TituloSlider.Name = "TituloSlider"
                                    TituloSlider.Parent = Slider
                                    TituloSlider.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    TituloSlider.BackgroundTransparency = 1.000
                                    TituloSlider.Size = UDim2.new(0, 429, 0, 28)
                                    TituloSlider.Font = Enum.Font.SourceSans
                                    TituloSlider.Text = title
                                    TituloSlider.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TituloSlider.TextScaled = true
                                    TituloSlider.TextSize = 14.000
                                    TituloSlider.TextWrapped = true
                                    TituloSlider.TextXAlignment = Enum.TextXAlignment.Left
                        
                                    local frame = TextLabel
                                    local button = BotaoSlider
                                
                                    
                                    TextLabel_2.FocusLost:Connect(function()
                                        if not CanUse then return end
                                        as = tonumber(TextLabel_2.Text)
    
                                        if as ~= nil then
                                            pcall(callback, as)
                                        end
                                    end)
                                    local TextLabel_2 = TextLabel_2
                                    local mouse = game.Players.LocalPlayer:GetMouse()
                                    local uis = game:GetService('UserInputService')
                                    button.MouseButton1Down:Connect(function()
                                        if not CanUse then return end
                                        num = 640
                                        bool = not NumerosComPonto
                                        if NumerosComPonto == true then
                                            Value = (((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue) or 0
                                        else
                                            Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                                        end
                                        
                                    
                                        pcall(function()
                                            callback(Value)
                                        end)
                                        
                                        moveconnection = mouse.Move:Connect(function()
                                            
                                            if NumerosComPonto == true then
                                                Value = (((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue) or 0
                                            else
                                                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                                            end
                                            
                                            if NumerosComPonto then
                                                TextLabel_2.Text = Value
                                                if string.find(TextLabel_2.Text, '.') then
                                                    local splited = TextLabel_2.Text:split('.')
                                                    if splited[1] ~= nil and splited[2] ~= nil then
                                                        NewText = splited[1]..'.'..string.sub(splited[2], 0,2)
                                                        TextLabel_2.Text = NewText
                                                    else
                                                        TextLabel_2.Text = Value
                                                    end
                                                else
                                                    TextLabel_2.Text = Value
                                                end
                                            else
                                                TextLabel_2.Text = Value
                                            end
                                            
                                            
                                            
                                            pcall(function()
                                                callback(Value)
                                            end)
                                            frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, num), 1, 0)
                                        end)
                                        releaseconnection = uis.InputEnded:Connect(function(Mouse)
                                            if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                                                if NumerosComPonto == true then
                                                    Value = (((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue) or 0
                                                else
                                                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / num) * frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                                                end
                                                pcall(function()
                                                    callback(Value)
                                                end)
                                                frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, num), 1, 0)
                                                moveconnection:Disconnect()
                                                releaseconnection:Disconnect()
                                            end
                                        end)
                                    end)
                                end
                        
                                return Items
                            end
                            
                            local function MIRGQCI_fake_script() -- Frame.Draggable 
                                local script = Instance.new('LocalScript', Frame)
                                local UserInputService = game:GetService("UserInputService") local gui = script.Parent local dragging local dragInput local dragStart local startPos local function update(input) local delta = input.Position - dragStart game:GetService('TweenService'):Create(gui, TweenInfo.new(0.5, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), {Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)}):Play() end gui.InputBegan:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then dragging = true dragStart = input.Position startPos = gui.Position input.Changed:Connect(function() if input.UserInputState == Enum.UserInputState.End then dragging = false end end) end end) gui.InputChanged:Connect(function(input) if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then dragInput = input end end) UserInputService.InputChanged:Connect(function(input) if input == dragInput and dragging then update(input) end end)
                                wait(2)
                                for i,v in pairs(script.Parent:GetDescendants()) do if v:IsA('TextLabel') or v:IsA('TextButton') then v.TextSize = v.TextSize + 1 end end 
                            end
                            coroutine.wrap(MIRGQCI_fake_script)()
                            
                            return Tabs
