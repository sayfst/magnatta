Humanoid.WalkSpeed = 23 if reverse == false then away = away + 0.01 else away = away - 0.01 end if away >=0.5 then reverse = true elseif away < -0.4 then reverse = false end Faca.Handle.CFrame = speaker.Character.Torso.CFrame * CFrame.new(1.8, away, -POS) Faca.Handle.Orientation = Faca.Handle.Orientation + Vector3.new(0, 50, 0) task.wait() end end) end EquiparFaca() local Equipped = false game:GetService('StarterGui'):SetCore('SendNotification', { Title = 'Sucess loaded FE slenderman'; Text = 'Binds:\n "F" = Equip/Unequip KNIFE\n"G" = Punch' }) speaker:GetMouse().KeyDown:Connect(function(Key) if Key:lower() == 'f' then Equipped = not Equipped if Equipped == true then GuardarFaca() else EquiparFaca() end elseif Key:lower() == 'g' then if Equipped == true then pcall(function() speaker.Character.HumanoidRootPart.Velocity = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.lookVector * 50 end); spawn(function() local part = Instance.new("Part", game.Players.LocalPlayer.Character) part.Transparency = 0 part.Size = Vector3.new(5, 2, 3) part.CanCollide = false local w1 = Instance.new("Weld", part) w1.Part0 = game.Players.LocalPlayer.Character.Torso w1.Part1 = part w1.C1 = CFrame.new(0,0,2) mainRemotes = game.ReplicatedStorage meleeRemote = mainRemotes['meleeEvent'] part.Touched:connect(function(hit) if game.Players:FindFirstChild(hit.Parent.Name) then local plr = game.Players:FindFirstChild(hit.Parent.Name) if plr.Name ~= game.Players.LocalPlayer.Name then part:Destroy() for i = 1,100 do meleeRemote:FireServer(plr) end end end end) end) end end end) end end)
                            else
                                NotifyCallback('SlenderMan FE Error', 'Error finding the necessary accessories', nil, 5, 'Copy Link', 'Ok', function()
                                    setclipboard('Hat 1: https://www.roblox.com/catalog/7390040662/Power-25\nHat 2: https://www.roblox.com/catalog/4855525473/Nagamaki\nTotal Robux needed: 125')
                                end, function()end)
                            end
                        else
                            Notify('Error', 'The script is for R6')
                        end
                    end)                
