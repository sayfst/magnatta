                                local name = '1'
                                if b == 0 then
                                    getHumanoid(speaker).Name = name
                                    getHumanoid(speaker).WalkSpeed = 16
                                elseif b == 1 then
                                    getHumanoid(speaker).Name = name
                                    getHumanoid(speaker).WalkSpeed = 16 + 5
                                elseif b == 2 then
                                    getHumanoid(speaker).Name = name
                                    getHumanoid(speaker).WalkSpeed = 16 + 10
                                elseif b == 3 then
                                    getHumanoid(speaker).Name = name
                                    getHumanoid(speaker).WalkSpeed = 16 + 15
                                elseif b == 4 then
                                    getHumanoid(speaker).Name = name
                                    getHumanoid(speaker).WalkSpeed = 16 + 20
                                elseif b == 5 then
