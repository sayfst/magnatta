                            Frame2.BackgroundTransparency = 0.5

                            Frame3.Name = "Frame3"
                            Frame3.Parent = Frame2
                            Frame3.BackgroundColor3 = Color3.fromRGB(79, 82, 87)
                            Frame3.BorderSizePixel = 0
