                    getgenv().R = 14
                    getgenv().G = 98
                    getgenv().B = 194
                    
                    getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)

                    
                    TabEsp:CreateTextBox('RGB CUSTOM', 'String',function(text)
                    
                        text = text:split(', ')
                        print('Color3.fromRGB('..text[1]..', '..text[2]..', '..text[3]..')')
                        getgenv().R = tonumber(text[1])
                        getgenv().G = tonumber(text[2])
                        getgenv().B = tonumber(text[3])
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - R', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().R = color
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - G', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().G = color
                    end)
                    
                    TabEsp:CreateSlider('RGB COLOR - B', 0, 255, function(color)
                        getgenv().Color = Color3.fromRGB(getgenv().R, getgenv().G, getgenv().B)
                        getgenv().B = color
                    end)

                    TabEsp:CreateWhile('Rainbow Esp', function(bool)
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(255/255,i/255,0/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(i/255,255/255,0/255)
                        end
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(0/255,255/255,i/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(0/255,i/255,255/255)
                        end
                        for i = 0,255,10 do
                            wait()
                            getgenv().Color = Color3.new(i/255,0/255,255/255)
                        end
                        for i = 255,0,-10 do
                            wait()
                            getgenv().Color = Color3.new(255/255,0/255,i/255)
                        end
                    end)
                    
                    speedBoost = 10
                    bindBoost = Enum.KeyCode.RightControl
                    Vehicle:CreateSlider('Vehicle Speed', 1, 10, function(value)
                        speedBoost = value
                    end, true)
                    
                    
                    
                    
                    alreadyBind = false
                    boostOn = false
                    Vehicle:CreateToggle('Boost ON', function(bool)
                        boostOn = bool
                    end)
                    Vehicle:CreateSlider('Vehicle gravity', 0, 196.2, function(value)
                        workspace.Gravity = value
                    end)
                    Vehicle:CreateButton('Bind key - Speed boost', function()
                        game.StarterGui:SetCore('SendNotification', {
                            Title = 'Tutorial';
                            Text = 'Press any key to set to Bind key';
                        })
                        local bind = game:GetService('UserInputService').InputBegan:Connect(function(input)
                            bindBoost = input.KeyCode
                            alreadyBind = true
                            game.StarterGui:SetCore('SendNotification', {
                                Title = 'Sucess set bind';
                                Text = 'Your new bind are ';
                            })
                        end)
                        repeat wait() until alreadyBind == true
                        alreadyBind = false
                        bind:Disconnect()
                    end)
                    running = false
                    game:GetService('UserInputService').InputBegan:Connect(function(input)
                        if input.KeyCode == bindBoost and boostOn then
                            running = true;
                            local Character = game.Players.LocalPlayer.Character; 
                            local Humanoid = Character:FindFirstChildWhichIsA("Humanoid"); 
                            local VehicleSeat = Humanoid.SeatPart; 
                            local Vehicle = VehicleSeat:FindFirstAncestorWhichIsA("Model"); 
                            if table.find({"wheels", "body", "misc", "core"}, Vehicle.Name) then 
                                Vehicle = Vehicle.Parent; 
                            end; 
                            local TweenService = game:GetService('TweenService')
                            for index, value in pairs(Vehicle:GetDescendants()) do
                                if value:IsA("BasePart") then 
                                    value.Velocity = value.CFrame * CFrame.new(0, 0, -speedBoost).Position
                                end; 
                            end;
                            task.wait(.5)
                        end                
                    end)
                    game:GetService('UserInputService').InputEnded:Connect(function(input)
                        if input.KeyCode == bindBoost then
                            running = false;
                        end
                        
                    end)
                    
                    
                    Vehicle:CreateButton('Vehicle Fly {H}', function()
                        pcall(function()
                            FLYSPEED = 50
                            repeat wait() until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:findFirstChild("Head") and game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid") local mouse = game.Players.LocalPlayer:GetMouse() repeat wait() until mouse local plr = game.Players.LocalPlayer local torso = plr.Character.Head local flying = false local deb = true local ctrl = {f = 0, b = 0, l = 0, r = 0} local lastctrl = {f = 0, b = 0, l = 0, r = 0} local maxspeed = 5000 local speed = 5000 function Fly() local bg = Instance.new("BodyGyro", torso) bg.P = 9e4 bg.maxTorque = Vector3.new(9e9, 9e9, 9e9) bg.cframe = torso.CFrame local bv = Instance.new("BodyVelocity", torso) bv.velocity = Vector3.new(0,0.1,0) bv.maxForce = Vector3.new(9e9, 9e9, 9e9) repeat wait() plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then speed = speed+.0+(speed/maxspeed) if speed > maxspeed then speed = maxspeed end elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then speed = speed-5 if speed > 5 then speed = -2 end end if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r} elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - game.Workspace.CurrentCamera.CoordinateFrame.p))*speed else bv.velocity = Vector3.new(0,0.1,0) end bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0) until not flying ctrl = {f = 0, b = 0, l = 0, r = 0} lastctrl = {f = 0, b = 0, l = 0, r = 0} speed = 5 bg:Destroy() bv:Destroy() plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false end mouse.KeyDown:connect(function(key) if key:lower() == "h" then if flying then flying = false else flying = true Fly() end elseif key:lower() == "w" then ctrl.f = FLYSPEED elseif key:lower() == "s" then ctrl.b = -FLYSPEED elseif key:lower() == "a" then ctrl.l = -FLYSPEED elseif key:lower() == "d" then ctrl.r = FLYSPEED end end) mouse.KeyUp:connect(function(key) if key:lower() == "w" then ctrl.f = 0 elseif key:lower() == "s" then ctrl.b = 0 elseif key:lower() == "a" then ctrl.l = 0 elseif key:lower() == "d" then ctrl.r = 0 end end) Fly()    
                        end);
                    end)
                    
                    Vehicle:CreateButton('Force stop vehicle', function()
                        local Character = game.Players.LocalPlayer.Character; 
                        local Humanoid = Character:FindFirstChildWhichIsA("Humanoid"); 
                        local VehicleSeat = Humanoid.SeatPart; 
                        local Vehicle = VehicleSeat:FindFirstAncestorWhichIsA("Model"); 
                        if table.find({"wheels", "body", "misc", "core"}, Vehicle.Name) then 
                            Vehicle = Vehicle.Parent; 
                        end; 
                        local TweenService = game:GetService('TweenService')
                        for index, value in pairs(Vehicle:GetDescendants()) do
                            if value:IsA("BasePart") then 
                                TweenService:Create(value, TweenInfo.new(0.25), {["Velocity"] = Vector3.new(0, 0, 0); ["RotVelocity"] = Vector3.new(0, 0, 0)}):Play(); 
                            end; 
                        end; 
                    end)
                    
                    Vehicle:CreateButton('Destroy vehicle', function()
                        local seat = game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid').SeatPart
                        local vehicleModel = seat.Parent
                        repeat
                            if vehicleModel.ClassName ~= "Model" then
                                vehicleModel = vehicleModel.Parent
                            end
                        until vehicleModel.ClassName == "Model"
                        wait(0.1)
                        vehicleModel:MoveTo(Vector3.new(999999, 9999999, 99999999))
                    end)
                    
                    
                    local AimbotConfig = {
                        AimbotOnline = false;
                        AimbotFovVisible = false;
                        AimbotFov = 50;
                        AimbotKey = Enum.KeyCode.LeftControl;
                        AimbotTeamCheck = false;
                    }
                    
                    
                    local AimPart = 'HumanoidRootPart'
                    
                    function AddAimbot(Aimbot)
                        Aimbot:CreateButton('Save aimbot settings', function()
                            writefile('Aimbot settings.lua', game:GetService('HttpService'):JSONEncode(AimbotConfig))
                            writefile('Aimpart.lua', game:GetService('HttpService'):JSONEncode({Parte = AimPart}))
                            
                        end)
                        Aimbot:CreateButton('Load aimbot settings', function()
                            local tabela = AimbotConfig
                        
                            local HttpService = game:GetService('HttpService')
                        
                            local sp = HttpService:JSONDecode(readfile('Aimbot settings.lua'))
                            local spp = HttpService:JSONDecode(readfile('Aimpart.lua'))
                        
                            tabela.AimbotOnline = sp.AimbotOnline
                            tabela.AimbotFovVisible = sp.AimbotFovVisible
                            tabela.AimbotFov = sp.AimbotFov
                            tabela.AimbotTeamCheck = sp.AimbotTeamCheck
                            AimPart = spp.Parte
                        end)
                        
                        Aimbot:CreateToggle('Aimbot', function(bool)
                            AimbotConfig.AimbotOnline = bool;
                        end)
                        
                        Aimbot:CreateToggle('Fov Visible', function(bool)
                            AimbotConfig.AimbotFovVisible = bool;
                        end)
                        
                        Aimbot:CreateToggle('Aimbot Team Check', function(bool)
                            AimbotConfig.AimbotTeamCheck = bool
                        end)
                        
                        Aimbot:CreateSlider('Fov size', 10, 1200, function(fov)
                            AimbotConfig.AimbotFov = fov;
                        end)
                        
                        Aimbot:CreateButton('AimPart: Head', function()
                            AimPart = 'Head'
                        end)
                        
                        Aimbot:CreateButton('AimPart: HumanoidRootPart', function()
                            AimPart = 'HumanoidRootPart'
                        end)
                        
                        Aimbot:CreateButton('AimPart: UpperTorso', function()
                            AimPart = 'UpperTorso'
                        end)
                        
                        Aimbot:CreateButton('AimPart: LowerTorso', function()
                            AimPart = 'LowerTorso'
                        end)

                        Aimbot:CreateToggle('AimPart: Random - Loop', function(bool)
                            RandomPart = bool
                            spawn(function()
                                while RandomPart == true do
                                    if R15(game.Players.LocalPlayer) then
                                        local Partes = {'HumanoidRootPart','Head','UpperTorso','LowerTorso'}
                        
                                        AimPart = Partes[math.random(1, #Partes)]
                                    else
                                        local Partes = {'HumanoidRootPart','Head','Torso', 'Left Arm', 'Right Arm', 'Left Leg', 'Right Leg'}
                        
                                        AimPart = Partes[math.random(1, #Partes)]
                                    end
                                    task.wait()
                                end
                            end)
                        end)
                        
                        MethodAim = 'CFrame'
        
                        Aimbot:CreateToggle('Method', function(bool)
                            if bool then MethodAim = 'TweenService' elseif not bool then MethodAim = 'CFrame' end
                        end)
                        
                        Aimbot:CreateButton('Aimbot Key', function()
                            alreadyBindAimbot = false;
                            local bind = game:GetService('UserInputService').InputBegan:Connect(function(input)
                                AimbotConfig.AimbotKey = input.KeyCode
                                alreadyBindAimbot = true;
                            end)
                        
                            repeat wait() until alreadyBindAimbot == true;
                            alreadyBindAimbot = false;
                            bind:Disconnect()
                        end)
                        
                        
                        local UserInputService = game:GetService('UserInputService')
                        
                        local Pressing = false;
                        
                        UserInputService.InputBegan:Connect(function(input)
                            if input.KeyCode == AimbotConfig.AimbotKey then
                                Pressing = true;
                            end
                            
                        end)
                        
                        UserInputService.InputEnded:Connect(function(input)
                            if input.KeyCode == AimbotConfig.AimbotKey then
                                Pressing = false;
                            end
                            
                        end)
                        
                        local camera = workspace.CurrentCamera
                        local mouse = game:GetService('Players').LocalPlayer:GetMouse()
                        
                        
                        
                        
                        local Fov = Drawing.new('Circle')
                        Fov.Visible = AimbotConfig.AimbotFovVisible
                        Fov.Thickness = 1
                        Fov.Color = getgenv().Color
                        Fov.Position = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 2)
                        Fov.Radius = AimbotConfig.AimbotFov
                        
                        game:GetService('RunService').RenderStepped:Connect(function()
                            Fov.Color = getgenv().Color
                            local dist = math.huge
                            Fov.Visible = AimbotConfig.AimbotFovVisible
                            Fov.Radius = AimbotConfig.AimbotFov
                            if AimbotConfig.AimbotOnline and Pressing then
                                for i,v in pairs(game.Players:GetChildren()) do
                                    
                                    if v ~= game.Players.LocalPlayer then
                                        if v.Character ~= nil and
                                        v.Character:FindFirstChild(AimPart) ~= nil and
                                        v.Character:FindFirstChild('Humanoid') ~= nil and
                                        v.Character:FindFirstChild('Humanoid').Health > 0 then
                        
                                            local Vector, OnScreen = camera:worldToViewportPoint(v.Character[AimPart].Position)
                        
                                            if OnScreen then
                                                local mag = (Vector2.new(mouse.X, mouse.Y) - Vector2.new(Vector.X, Vector.Y)).Magnitude
                                                local PlayersMag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Character.HumanoidRootPart.Position).Magnitude
                                                
                                                if PlayersMag < dist and mag <= AimbotConfig.AimbotFov then
                                                    dist = PlayersMag
                                                    function mirar()
                                                        if MethodAim == 'CFrame' then
                                                            camera.CFrame = CFrame.new(camera.CFrame.Position, v.Character[AimPart].Position)
                                                        else 
                                                            game:GetService('TweenService'):Create(camera, TweenInfo.new(0.1), {CFrame = CFrame.new(camera.CFrame.Position, v.Character[AimPart].Position)}):Play()
                                                        end
                                                    end
                                                    if AimbotConfig.AimbotTeamCheck == true then
                                                        if game.Players.LocalPlayer.TeamColor ~= v.TeamColor then
                                                            mirar()
                                                        end
                                                    else
                                                        mirar()
                                                    end
                                                end
                                            end                
                                        end
                                    end
                                end
                            end
                        end)
                    end

                    AddAimbot(Aimbot)
                    
                    if game.PlaceId == 155615604 then
                        game:GetService('RunService').RenderStepped:Connect(function()
                            if game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y < -10 then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 150, 0)
                            end
                        end)
                    end
    
                    local RandomPart = false
                    
                    Server:CreateButton('.lua executor', function()
                        local foldername = 'Script hub'
                        local path = '.lua'
                        if not isfolder(foldername) then
                            makefolder(foldername)
                        end
                        
                        local pasta = foldername..'/'
                        
                        local Frame = Instance.new("ScreenGui")
                        
                        local Titulo = Instance.new("TextLabel")
                        Titulo.Active = true
                        Titulo.Draggable = true
                        local Frame_2 = Instance.new("Frame")
                        local UIListLayout = Instance.new("UIListLayout")
                        local findFile = Instance.new("TextBox")
                        
                        
                        
                        
                        function CreateButton(titulo, callback)
                            local execute = Instance.new("TextButton")
                        
                            execute.Name = "execute"
                            execute.Parent = Frame_2
                            execute.BackgroundColor3 = Color3.fromRGB(47, 13, 13)
                            execute.BorderSizePixel = 0
                            execute.Size = UDim2.new(0, 233, 0, 18)
                            execute.Font = Enum.Font.SourceSans
                            execute.Text = titulo..path
                            execute.TextColor3 = Color3.fromRGB(223, 223, 223)
                            execute.TextSize = 14.000
                            execute.Visible = true
                            execute.MouseButton1Click:Connect(function()
                                pcall(callback)
                            end)
                        end
                        
                        Frame.Name = "Frame"
                        Frame.Parent = game.CoreGui
                        Frame.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                        game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(b)
                            if b:lower() == 'p' then
                                Frame.Enabled = not Frame.Enabled
                            end
                        end)
                        Titulo.Name = "Titulo"
                        Titulo.Parent = Frame
                        Titulo.BackgroundColor3 = Color3.fromRGB(29, 9, 9)
                        Titulo.BorderSizePixel = 0
                        Titulo.Position = UDim2.new(0.133423179, 0, 0.104294479, 0)
                        Titulo.Size = UDim2.new(0, 233, 0, 27)
                        Titulo.Font = Enum.Font.Sarpanch
                        Titulo.Text = ".Lua"
                        Titulo.TextColor3 = Color3.fromRGB(255, 255, 255)
                        Titulo.TextScaled = true
                        Titulo.TextSize = 31.000
                        Titulo.TextWrapped = true
                        
                        Frame_2.Parent = Titulo
                        Frame_2.BackgroundColor3 = Color3.fromRGB(35, 7, 7)
                        Frame_2.BorderSizePixel = 0
                        Frame_2.Position = UDim2.new(0, 0, 1.55555558, 0)
                        Frame_2.Size = UDim2.new(0, 233, 0, 190)
                        
                        UIListLayout.Parent = Frame_2
                        UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                        UIListLayout.Padding = UDim.new(0, 5)
                        
                        
                        
                        findFile.Name = ""
                        findFile.Parent = Titulo
                        findFile.BackgroundColor3 = Color3.fromRGB(42, 10, 10)
                        findFile.BorderSizePixel = 0
                        findFile.Position = UDim2.new(0, 0, 1, 0)
                        findFile.Size = UDim2.new(0, 233, 0, 15)
                        findFile.Font = Enum.Font.SourceSans
                        findFile.PlaceholderColor3 = Color3.fromRGB(98, 98, 98)
                        findFile.PlaceholderText = "File name"
                        findFile.Text = ""
                        findFile.TextColor3 = Color3.fromRGB(255, 255, 255)
                        findFile.TextSize = 14.000
                        findFile.Changed:Connect(function(text)
                            if text == 'Text' then
                                if findFile.Text == 'Reset' then
                                    findFile.Text = ''
                                    for i,v in pairs(Frame_2:GetChildren()) do
                                        if v ~= findFile and v ~= UIListLayout then
                                            v:Destroy()
                                        end
                                    end
                                    loadFiles()
                                else
                                    for i,v in pairs(Frame_2:GetChildren()) do
                                        if v ~= findFile and v ~= UIListLayout then
                                            if string.find(v.Text, findFile.Text) then
                                                v.Visible = true
                                            else
                                                v.Visible = false
                                            end
                                        end
                                    end
                                end
                            end
                        end)
                        
                        function loadFiles()
                            local files = listfiles(foldername)
                        
                            for i,v in pairs(files) do
                                local HttpService = game:GetService('HttpService')
                            
                                local json = HttpService:JSONDecode(readfile(v))
                            
                                CreateButton(string.gsub(tostring(v), foldername..'/', ''), function()
                                    pcall(function()
                                        loadstring(json)()
                                    end)
                                end)
                            end
                        end
                        
                        loadFiles()
                        
                        local ScreenGui = Instance.new("ScreenGui")
                        local Frame = Instance.new("Frame")
                        local TextLabel = Instance.new("TextLabel")
