                    Notify('Magnatta v2', 'Version: '..verSAO, nil, 4)
                    function CheckGame(table)
                        if typeof(table) ~= 'table' then
                            if game.PlaceId == table then
                                return true
                            else
                                return false
                            end
                        end
                        for i,v in pairs(table) do
                            if game.PlaceId == v then
                                return true;
                            end
                        end
                        return false;
                    end
                    
                    
                    
