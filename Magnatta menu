                        --[[Murder mystery 2]] if game.PlaceId == 142823291 then
                            ChangeName('Murder mystery 2')
                            SupportGame:CreateButton('Murder Mystery Esp', function()
                                Notify(
                                    'Murder mystery esp',
                                    'Made by me',
                                    getImage('Bell'),
                                    5
                                )
                                
                                local camera = workspace.CurrentCamera
                                function on(plr)
                                    if plr ~= game.Players.LocalPlayer then
                                        local Line = Drawing.new('Line')
                                        Line.Visible = false
                                        Line.Thickness = 0.3
                                
    
                                        spawn(function()
                                            game.Players.PlayerRemoved:Connect(function(b)
                                                if b == plr then
                                                    Line:Remove()
                                                end
                                            end)
                                        end)
                                        function update_esp_position()
                                            game:GetService('RunService').Heartbeat:Connect(function()
                                                local v = plr
                                                if v.Character ~= nil and
                                                v.Character:FindFirstChild('Humanoid') ~= nil and
                                                v.Character:FindFirstChild('Humanoid').Health > 0 then
                                                    if FindTool('Knife', v) then
                                                        Line.Color = Color3.fromRGB(255, 0, 0)
                                                    elseif FindTool('Gun', v) then
                                                        Line.Color = Color3.fromRGB(0, 0, 255)
                                                    else
                                                        Line.Color = Color3.fromRGB(0, 255, 0)
                                                    end
                                                    local Vector, OnScreen = camera:worldToViewportPoint(v.Character:FindFirstChild('HumanoidRootPart').Position)
                                
                                                    if OnScreen then
                                                        Line.Visible = EspMurderOn
                                                        Line.From = Vector2.new(camera.ViewportSize.X / 2, camera.ViewportSize.Y / 1)
                                                        Line.To = Vector2.new(Vector.X, Vector.Y)
                                                    else
                                                        Line.Visible = false
                                                    end
                                                end
                                            end)
                                        end
                                        coroutine.wrap(update_esp_position)()
                                    end
                                end
                                
                                for i,plr in pairs(game.Players:GetChildren()) do
                                    on(plr)
                                end
                                game.Players.PlayerAdded:Connect(function(plr)
                                on(plr)
                                end)
                            end)
                            SupportGame:CreateToggle('Esp visible', function(bool)
                                EspMurderOn = bool
                            end)
                            SupportGame:CreateButton('Select murder', function()
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if FindTool('Knife', v) then
                                        target = v
                                    end
                                end
                            end)
                            SupportGame:CreateButton('Select sheriff', function()
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if FindTool('Gun', v) then
                                        target = v
                                    end
                                end
                            end)
                            SupportGame:CreateButton('Kill all', function()
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if v ~= speaker and not CheckPlayerIsWhitelist(v) and getRoot(v) then
                                        local step = false
                                        a = game:GetService('UserInputService').InputBegan:Connect(function(input)
                                            if input.KeyCode == Enum.KeyCode.LeftControl then
                                                a:Disconnect()
                                                step = true
                                            end
                                        end)
                                        repeat
                                            getRoot(speaker).CFrame = getRoot(v).CFrame * CFrame.new(0, 0, 1)
                                            if speaker.Character:FindFirstAncestorWhichIsA('Tool') then
                                                speaker.Character:FindFirstAncestorWhichIsA('Tool'):Activate()
                                            end
                                            task.wait()
                                        until v.Character.Humanoid.Health == 0 or step == true
                                        if not step then
                                            a:Disconnect()
                                        end
