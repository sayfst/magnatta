                        function noclipSupport(t)
                            isSupportNoclip = t
                            if syn then
                                setfflag("HumanoidParallelRemoveNoPhysics", "False")
                                setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
                                if t == true then
                                    game:GetService('RunService'):BindToRenderStep("crash2", 0 , function()
                                        game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
                                    end)
                                elseif t == false then
                                    game:GetService('RunService'):UnbindFromRenderStep("crash2")
                                end
                            elseif KRNL_LOADED then
                                if t == true then
                                    function lNoclip()
                                        for i,v in pairs(getCharacter(speaker):GetDescendants()) do
                                            if v:IsA('BasePart') and v.CanCollide == true then
                                                v.CanCollide = false
                                            end
                                        end
                                    end

                                    noclip5 = game.RunService.RenderStepped:Connect(lNoclip)
                                    repeat wait() until isSupportNoclip == false
                                    noclip5:Disconnect()
                                end
                            end
                        end
                        function Korblox()
                            local ply = game.Players.LocalPlayer
                            local chr = ply.Character
                            chr.RightLowerLeg.MeshId = "902942093"
                            chr.RightLowerLeg.Transparency = "1"
                            chr.RightUpperLeg.MeshId = "http://www.roblox.com/asset/?id=902942096"
                            chr.RightUpperLeg.TextureID = "http://roblox.com/asset/?id=902843398"
                            chr.RightFoot.MeshId = "902942089"
                            chr.RightFoot.Transparency = "1"
                        end
                        function HeadLess()
                            game.Players.LocalPlayer.Character.Head.Transparency = 1
                            game.Players.LocalPlayer.Character.Head:FindFirstChildWhichIsA('Decal').Transparency = 1
                        end
                        function Animation()
                            if not game:IsLoaded() then
                                game.Loaded:wait()
