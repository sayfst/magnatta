                        local lowerTorso = char:WaitForChild('LowerTorso')
                        oldposss = humanoid.RootPart.CFrame
                        humanoid.RootPart.CFrame = CFrame.new(0, 100, 0)
                        
                        wait(.1)
                        
                        lowerTorso:Destroy()
                        
                        for i,v in pairs(char:GetChildren()) do
                            if v:IsA('BasePart') and v.Name ~= humanoid.RootPart.Name then
                                task.spawn(function()
                                    repeat
                                        v.CFrame = CFrame.new(0, 100, 0)
                                        v.Velocity = Vector3.new(0, 5, 0)
                                        wait()
                                    until humanoid.Health == 0
                                end)
                            end
                        end
                        humanoid.RootPart.CFrame = oldposss
                    end)
