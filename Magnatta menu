                    function Refresh(pleasewait)
                        pleasewait = pleasewait or false
                        if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Torso") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Torso"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Left Arm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Left Arm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Right Arm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Right Arm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Left Leg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Left Leg"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Right Leg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Right Leg"):Destroy()
                        end                        
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightHand") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightHand"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftLowerArm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftLowerArm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightLowerArm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightLowerArm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftUpperArm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftUpperArm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightUpperArm") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightUpperArm"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LowerTorso"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftLowerLeg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftLowerLeg"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftUpperLeg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftUpperLeg"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftFoot") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftFoot"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightFoot") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightFoot"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightLowerLeg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightLowerLeg"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("RightUpperLeg") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("RightUpperLeg"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("UpperTorso"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("LeftHand") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("LeftHand"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Head") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Head"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Animate") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Animate"):Destroy()
                        end
                        if game.Players.LocalPlayer.Character:FindFirstChild("Health") ~= nil then
                            game.Players.LocalPlayer.Character:FindFirstChild("Health"):Destroy()
                        end
                        if pleasewait then
                            speaker.CharacterAdded:Wait()
                        end
                    end
                    local OldPos = nil
                    local OldPos2 = nil
                    getgenv().RefreshLoop = false
                    if  game.Players.LocalPlayer.Character:FindFirstChild('HumanoidRootPart') == nil then
                        spawn(function()
                            game.Players.LocalPlayer.Character:WaitForChild('HumanoidRootPart')
                            OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                        end)
                    else
                        OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                        OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                    end
                    Player:CreateButton('Alejado', function()
                        game.Players.LocalPlayer.Character["RightHand"]:Destroy() game.Players.LocalPlayer.Character["LeftLowerArm"]:Destroy() game.Players.LocalPlayer.Character["RightLowerArm"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperArm"]:Destroy() game.Players.LocalPlayer.Character["RightUpperArm"]:Destroy()  game.Players.LocalPlayer.Character["LeftLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftUpperLeg"]:Destroy() game.Players.LocalPlayer.Character["LeftFoot"]:Destroy() game.Players.LocalPlayer.Character["RightFoot"]:Destroy() game.Players.LocalPlayer.Character["RightLowerLeg"]:Destroy() game.Players.LocalPlayer.Character["RightUpperLeg"]:Destroy()  game.Players.LocalPlayer.Character["LeftHand"]:Destroy()   
                    end)
                    Player:CreateToggle('Loop Refresh', function(bool)
                        getgenv().RefreshLoop = bool
                    
                        if bool then
                            local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                            local OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                    
                            repeat
                                pcall(function()
                                    local OldPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
                                    local OldPos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                                    game.Players.LocalPlayer.Character.Humanoid.Health = 0
                                    spawn(function()
                                        game.Players.LocalPlayer.CharacterAdded:wait():WaitForChild('HumanoidRootPart')
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = OldPos
                                        wait(.5)
                                    end)
                                    Refresh() 
                                end)
                                task.wait()
                            until getgenv().RefreshLoop == false
                        end
                    end)
                    
                    
                    
                    Player:CreateButton('Tool reach', function(bool)
                        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
