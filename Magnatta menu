        ImageLabel.Position = UDim2.new(0, 0, 0.333333343, 0)
        ImageLabel.Size = UDim2.new(0, 36, 0, 36)
        ImageLabel.ZIndex = 50
        if icon ~= nil then
            ImageLabel.MouseButton1Click:Connect(function()
                ImageLabel:TweenSize(UDim2.new(0, 200, 0, 200), 'InOut', 'Quart', 1, true)
                ImageLabel:TweenPosition(UDim2.new(-2, 0, -5, 0), 'InOut', 'Quart', 1, true)
                wait(4)
                ImageLabel:TweenPosition(UDim2.new(0, 0, -1, 0), 'InOut', 'Quart', 1, true)
                ImageLabel:TweenSize(UDim2.new(0, 36, 0, 36), 'InOut', 'Quart', 1, true)
            end)
            ImageLabel.Image = icon
        end
        task.spawn(function()
            wait(duration)
            TextLabel:Destroy()
        end)
    end
end)
local function FindPlayer(String)
    for _, _Player in pairs(game.Players:GetPlayers()) do
        if _Player.Name:lower():match('^'.. String:lower()) or _Player.DisplayName:lower():match('^'.. String:lower()) then
            return _Player
        end
    end
    return nil
end
function mathString(string1,string2)
    string1 = string1:lower()
    string2 = string2:lower()
    
    if string.find(string2, string1) then
        return true
    else
        return false
    end
end



local mt = getrawmetatable(game)
local old = mt.__namecall
local protect = newcclosure or protect_function
local Players = game.Players


if not protect then
    game.StarterGui:SetCore('SendNotification', {
        Title = 'Error';
        Text = 'Error to find protect';
    })
else
    setreadonly(mt, false)
    mt.__namecall = protect(function(self, ...)
        local method = getnamecallmethod()
        if method == "Kick" then
            wait(9e9)
            return
        end
        return old(self, ...)
    end)
    hookfunction(Players.LocalPlayer.Kick,protect(function() wait(9e9) end))

end

function mag()
    local ver = '2.3'
    _G.Desinject = function()
        for i,v in pairs(game:GetService('CoreGui'):GetChildren()) do
            if v:FindFirstChild('Magnatta') then
                v:Destroy()
            end
        end
    end
    _G.Desinject()
    local Keys = loadstring(game:HttpGet('https://pastebin.com/raw/fCUBF3F7'))()
    local SavedKey = 'keyLoaded2.lua'
    local CanUse = true
    
    
    
    
    getgenv().Ver = 'NOT SELECT'
    
    verSAO = ver
    function NotifyCallback(title, text, icon, duration, button1, button2, function1, function2)
        title = title or ''
        text = text or ''
        icon = icon or nil
        duration = duration or 1
        button1 = button1 or 'Button1'
        button2 = button2 or 'Button2'
        function1 = function1 or function()end
        function2 = function2 or function()end
    
        local callback = function(text)
            if text == button1 then
                pcall(function1)
            elseif text == button2 then
                pcall(function2)
            end
        end
    
        local BindableFunction = Instance.new('BindableFunction')
        BindableFunction.OnInvoke = callback 
    
        game.StarterGui:SetCore('SendNotification', {
            Title = title;
            Text = text;
            Icon = icon;
            Duration = duration;
            Button1 = button1;
            Button2 = button2;
            Callback = BindableFunction;
        })
    end
    pcall(function()
        game.RunService.RenderStepped:Connect(function()
            if game.PlaceId == 155615604 then            
                game:GetService("Players").LocalPlayer.PlayerGui.Home.fadeFrame.BackgroundTransparency = 1
            end
        end)
    end);
    if (rconsoleprint and rconsoleinput and rconsoleinfo and isfile and writefile and readfile) then
        if (syn or KRNL_LOADED) then
            local blackListed = false
            local MouseName = 'Magnatta Mouse'
            function MouseChange(bool)
                mouse_online = bool
                if getgenv().CanMouse == false or getgenv().CanMouse == nil then 
                    local uis = game:GetService('UserInputService')
                    uis.MouseBehavior = oldbehavior
                    uis.MouseIconEnabled = oldiconenabled
                    uis.MouseEnabled = oldenabled
                    return 
                end
                if bool then
                    uis = game:GetService('UserInputService')
                    local value = uis.MouseEnabled
                    oldenabled = value
                    oldiconenabled = uis.MouseIconEnabled
                    oldbehavior = uis.MouseBehavior

                    uis.MouseIconEnabled = false
                    uis.MouseBehavior = Enum.MouseBehavior.Default
                    local mouse = Instance.new("ScreenGui")
                    local plus = Instance.new("ImageLabel")
                    
                    mouse.Name = "mouse"
                    mouse.Parent = game.CoreGui
                    mouse.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
                    
                    plus.Name = "plus"
                    plus.Parent = mouse
                    plus.BackgroundTransparency = 1.000
                    plus.Position = UDim2.new(0.400000006, 0, 0.5, 0)
                    plus.Active = false
                    plus.Selectable = true
                    plus.Size = UDim2.new(0, 15, 0, 15)
                    plus.ZIndex = 2
                    plus.Image = "rbxassetid://6764432408"
                    plus.ImageRectOffset = Vector2.new(100, 450)
                    plus.ImageRectSize = Vector2.new(50, 50)
                    
                    task.spawn(function()
                        local player = game.Players.LocalPlayer
                        local mouse2 = player:GetMouse()
                        local pos
                        repeat
                            pos = UDim2.new(0, mouse2.X - 8, 0, mouse2.Y - 8)
                            plus.Position = pos
                            task.wait()	
                        until mouse_online == false
                        mouse:Destroy()
                    end)
                end
            end
            pcall(function()
                local reason = 'No reason'
                local BlackListTable = loadstring(game:HttpGet('https://pastebin.com/raw/nb3saJtE'))()
                for i,v in pairs(BlackListTable) do
                    if game.Players.LocalPlayer.Name == v.Nome then
                        blackListed = true;
                        v.Reason = v.Reason or 'No reason'
                        reason = v.Reason
                    end
                end
            
                if blackListed == true then
                    rconsoleprint('@@RED@@')
                    rconsoleinfo('Error')
                    rconsoleprint("You're on blacklist Of Magnatta Menu\nReason: "..reason)
                    wait(5)
                    game.Players.LocalPlayer:Kick()
                    game:Shutdown()
                end
            end)
            
            function isNumber(stg)
                if tonumber(stg) ~= nil or stg == 'inf' or stg == math.huge then
                    return true;
                end
                return false
            end
            
            spawn(function()
                repeat
                    wait()
                until blackListed == false
                function MAGNATA_ACTIVE()
                    spawn(function()
                        local getError = true
                        while getError == true do
                            if isfile(SavedKey) then
                                local key = game:GetService('HttpService'):JSONDecode(readfile(SavedKey))
                                local KEYVALID = false
                                for i,v in pairs(Keys) do
                                    if v == key.KeyMagnatta then
                                        KEYVALID = true
                                    end
                                end
        
                                if KEYVALID == false then
                                    _G.Desinject()
                                    mag()
                                end
                            else
                                _G.Desinject()
                                mag()
                            end
                            wait(1)
                        end
                    end)
                    getgenv().Ver = '2'
                    local magnata = {}
            
                    function magnata:CreateWindow(titleWindow, config)
                        if getgenv().Ver == '2' then
                            local ScreenGui = Instance.new("ScreenGui")
                            local Frame = Instance.new("TextLabel")
                            local Sombra = Instance.new("Frame")
                            local umbraShadow = Instance.new("ImageLabel")
                            local penumbraShadow = Instance.new("ImageLabel")
                            local ambientShadow = Instance.new("ImageLabel")
                            local Frame1 = Instance.new("Frame")
                            local Frame2 = Instance.new("Frame")
                            local Frame3 = Instance.new("Frame")
                            local Frame4 = Instance.new("Frame")
