                    function CheckNumberString(string)
                        string = tostring(string) or ''
    
                        return #string
                    end
                    local isCrimnal = false
                    function CriminalP()
                        if isCrimnal == false then
                            isCrimnal = true
                            local OldPOS = game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.CFrame
                
                            local player = game.Players.LocalPlayer.Character.HumanoidRootPart
                            
                            repeat
                                game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.Transparency = 1
                            game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.CanCollide = false
                            
                            game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.CFrame = player.CFrame
                            task.wait()
                            until game.Players.LocalPlayer.TeamColor == BrickColor.new('Really red')
                            game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.Transparency = 0
                            game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.CanCollide = true
                            game:GetService("Workspace")["Criminals Spawn"].SpawnLocation.CFrame = OldPOS
                            isCrimnal = false
                        end
                    end
    
                    EtcTarget = nil
                    function ErrorEtc()
                        game.StarterGui:SetCore('SendNotification', {
                            Title = 'Error;';
                            Text = 'Please select a player';
                        })
                    end
    
                    Etc:CreateTextBox('Target name', 'String', function(value)
                        for i,v in pairs(game.Players:GetChildren()) do
                            if v ~= speaker then
                                if v.Name:lower():match(value:lower()) or v.DisplayName:lower():match(value:lower()) then
                                    EtcTarget = v;
                                    game.StarterGui:SetCore('SendNotification', {
                                        Title = 'Select';
                                        Text = v.Name
                                    })
                                end
                            end
                        end
                    end)
                    local NotNil = false
                    spawn(function()
                        while true do
                            if EtcTarget ~= nil then
                                NotNil = true
                            else
                                NotNil = false
                            end
                            task.wait()
                        end
                    end)
                    function verifyCharacter(v)
                        if v.Character ~= nil and
                        v.Character:FindFirstChild('HumanoidRootPart') ~= nil and
                        v.Character:FindFirstChild('Humanoid') ~= nil and v.Character:FindFirstChild('Head') ~= nil then
                            return true;
                        end
                        return false;
                    end
