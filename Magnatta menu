                            ChangeName('Hexa game Lobby')
    
                            SupportGame:CreateButton('Force auto start game', function()
                                local GameId = 7562422641
                                teleportToPlace(GameId)
                            end)
                        end
                        --[[Hexa game Game]] if game.PlaceId == 7562422641 then
                            ChangeName('Hexa game Game')
    
                            SupportGame:CreateButton('Finish game 1', function()
                                local root = getRoot(speaker)
    
                                root.CFrame = CFrame.new(-572.3756713867188, 4.007529258728027, 289.3094482421875)
                            end, corlegal)
                            local TimeToWait = 0.4
                            SupportGame:CreateToggle('Auto pull rope', function(b)
                                _G.AutoRope = b
                                if b then
                                    while _G.AutoRope == true do
                                        game:GetService("ReplicatedStorage").RE.Pull:FireServer()
                                        wait(.4)
                                    end
                                end
                            end)
                            SupportGame:CreateSlider('Auto pull rope Delay', 0, 1, function(bool)
                                TimeToWait = bool
                            end, true)
                            
                            SupportGame:CreateButton('Punch players', function()
                                spawn(function()
                                    while wait() do
                                        for i, v in next, game.Players:GetChildren() do
                                            if v.Character and v.Character:FindFirstChild"HumanoidRootPart" and v ~= game.Players.LocalPlayer then
                                                local punch = game.Players.LocalPlayer.Character.Punch
                                                firetouchinterest(punch.Handle, v.Character.HumanoidRootPart, 0)
                                                firetouchinterest(punch.Handle, v.Character.HumanoidRootPart, 1)
                                            end
                                        end 
                                    end
                                end)
                            end)
                            SupportGame:CreateButton('Esp Glases broken', function()
                                for i,v in pairs(game:GetService("Workspace").Glass:GetDescendants()) do
                                    if v.ClassName == "TouchTransmitter" then
                                        local BillboardGui = Instance.new("BillboardGui")
                                        local TextLabel = Instance.new("TextLabel")
                            
                                        BillboardGui.Parent = v.Parent
                                        BillboardGui.AlwaysOnTop = true
                                        BillboardGui.LightInfluence = 1
                                        BillboardGui.Size = UDim2.new(0, 50, 0, 50)
                                        BillboardGui.StudsOffset = Vector3.new(0, 2, 0)
                            
                                        TextLabel.Parent = BillboardGui
                                        TextLabel.BackgroundColor3 = Color3.new(1, 1, 1)
                                        TextLabel.BackgroundTransparency = 1
                                        TextLabel.Size = UDim2.new(1, 0, 1, 0)
                                        TextLabel.Text = "FakeGlass"
                                        TextLabel.TextColor3 = Color3.new(1, 0, 0)
                                        TextLabel.TextScaled = true
                                    end
                                end
                            end)
                        end
                    else
                        local games = {
                            {'Prison life';'https://www.roblox.com/games/155615604/Prison-Life-Cars-fixed'},
                            {'Adopt and Raise a baby';'https://www.roblox.com/games/383793228/Adopt-and-Raise-a-Baby'},
                            {'Minerscave';'https://www.roblox.com/games/6604417568/Minerscave-1-5'},
                            {'Mega Obby Parkour 550 Stages!';'https://www.roblox.com/games/8003084678/oMega-Obby-Parkour-550-Stages'},
                            {'Build a boat for Treasure';'https://www.roblox.com/games/537413528/Build-A-Boat-For-Treasure'},
                            {'3008 [2.7]';'https://www.roblox.com/games/2768379856/3008-2-7'},
                            {'Phantom Forces';'https://www.roblox.com/games/292439477/Phantom-Forces-May-Update'},
                            {'Map kat';'https://www.roblox.com/games/621129760/Knife-Ability-Test'},
                            {'Hexa game (Lobby)';'https://www.roblox.com/games/7555007091/VC-Hexa-Game'},
                            {'Hexa game (Game)';'https://www.roblox.com/games/7555007091/VC-Hexa-Game'},
                            {'Raise a floppa';'https://www.roblox.com/games/9203864304/raise-a-floppa'},
                            {'Murder mystery 2';'https://www.roblox.com/games/142823291/Murder-Mystery-2'},
                            {'Legends Of Speed';'https://www.roblox.com/games/3101667897/Legends-Of-Speed'},
                            {'Sword Fight on the Heights';'https://www.roblox.com/games/487316/Sword-Fight-on-the-Heights-Original'},
                            {'Clicker Simulator!';'https://www.roblox.com/games/7560156054/2-HOURS-Clicker-Simulator'},
                            {'Snowman Simulator';'https://www.roblox.com/games/2533391464/EGG-HUNT-Snowman-Simulator'},
                            {'Adopt me';'https://www.roblox.com/games/920587237/Adopt-Me'},
                            {'Isle';'https://www.roblox.com/games/3095204897/Isle#!/game-instances'},
                            {'Map kat';'https://www.roblox.com/games/621129760/Knife-Ability-Test'},
                            {'Da hood', 'https://www.roblox.com/games/2788229376/Da-Hood'},
                            {'Zombie Attack', 'https://www.roblox.com/games/1240123653/Zombie-Attack'},
                            {'Tower of heaven', 'https://web.roblox.com/games/6335256554/Tower-of-Heaven-FNF-STAGES'},
                            {'Ninja legends', 'https://www.roblox.com/games/3956818381/Ninja-Legends'},
                            {'Fall block', 'https://www.roblox.com/games/6155643742/Fall-Block'}
                        


                        }
                        text = ''
                        num = 0
                        for i,v in pairs(games) do
                            if v[1] and v[2] then
                                num = num + 1
                                text = text .. '\n['..num..'] = '..v[1]
                            end
                        end
                        
                        
                        if (setclipboard) then
                            Notify(
                            'This game is not in our game support.',
                            'We copied the name \nof all games on your clipboard!',
                            getImage('Bell'),
                            15
                            )
                            rconsoleprint('\nMagnatta Menu (Total games support: '..num..')\n'..text)
                            setclipboard('\nMagnatta Menu (Total games support: '..num..')\n'..text)
                        
                            rconsoleprint('\nWrite the name of a game above, to receive its link!\n\n')
                            wait()
                            while CanUse == true do
                                local b = rconsoleinput()
                                for i,v in pairs(games) do
                                    if b == v[1] then
                                        rconsoleprint(v[2]..'\n')
                                        setclipboard(v[2])
                                    end
                                end
                            end
                        end
                    end
                    
                    
                end
                
                
                
                function ESPERAR_RANDOM(salvar)
                    salvar = salvar or false
                    local numbers = {
                        1.1,1.2,1.3,1.4,1.5,1.6,1.7,1.8,1.9,2,2.1,2.2,2.3,2.4,2.5,2.6,2.7,2.8,2.9,3
                    }
                    local select = numbers[math.random(1, #numbers)]
                    
                    if salvar then
                        getgenv().TimeSelectLoad =select
                    end
                end
                local cors = {
                    '@@BLUE@@',
                    '@@RED@@',
                    '@@YELLOW@@'
                }
                local cor = 1
                local maxcor = #cors

                function CustomPrint(info, message, info_color, old_color, pairs_color)
                    old_color = '@@WHITE@@'
                    rconsoleprint('\n')  
                    rconsoleprint(pairs_color)  
                    rconsoleprint('[') rconsoleprint(info_color) rconsoleprint(info) rconsoleprint(old_color) rconsoleprint(pairs_color) rconsoleprint(']') rconsoleprint(old_color) rconsoleprint(' '..message)
                end

                
                function KEYSYSTEM(ALREADYPUT)
                    
                    rconsoleclear()
                    rconsolename('Magnatta Menu V3 (Whitelist system)')
                    rconsoleinfo('Magnatta v3')
                   
                    local Ativado = false
                    
                    
                    
                    if ALREADYPUT == false then
                        CustomPrint('WELCOME', 'Magnatta Menu v3', '@@LIGHT_CYAN@@', '@@BLUE@@', '@@WHITE@@')
                    end
                    CustomPrint('Whitelist', 'Please Put Your whitelist Key Down\nKey: ', '@@LIGHT_CYAN@@', '@@BLUE@@', '@@WHITE@@')
                    local KEYPUT = rconsoleinput()
                    
                    for i,v in pairs(Keys) do
                        if v == KEYPUT then
                            Ativado = true
                        end
                    end
                    
    
                    if Ativado == true then
                        writefile(SavedKey, game:GetService('HttpService'):JSONEncode({KeyMagnatta = KEYPUT}))
                        rconsoleclear()
                        
                        MAGNATA_ACTIVE()
                    else
                        CustomPrint('Error', 'Invalid Key', '@@LIGHT_RED@@', '@@BLUE@@', '@@WHITE@@')
                        wait(1)
                        KEYSYSTEM(true)
                    end
                end
                
                
                if (readfile and writefile and isfile) then
                    if isfile(SavedKey) then
                        local key = game:GetService('HttpService'):JSONDecode(readfile(SavedKey))
                        local KEYVALID = false
                        for i,v in pairs(Keys) do
                            if v == key.KeyMagnatta then
                                KEYVALID = true
                            end
                        end
                        if KEYVALID == false then
                            KEYSYSTEM(true)
                        elseif KEYVALID == true then
                            MAGNATA_ACTIVE()
                        end
                    else
                        KEYSYSTEM(false)
                    end
                end
            end)
        end
    end
end

mag()
