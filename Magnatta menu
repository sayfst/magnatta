                        FlingAura = bool 
                    end)
                    Freatures:CreateButton('Bypass fling', function()
                        for i,v in pairs(game.Players:GetChildren()) do 
                            if getRoot(v) then
                                game.RunService.RenderStepped:Connect(function()
                                    for i,v in pairs(v.Character:GetChildren()) do
                                        if v.CanCollide ~= false then
                                            v.CanCollide = false
                                        end
                                    end
                                end)
                            end
                        end
                    end)
                    Freatures:CreateToggle('Jogar player do mapa (Aura)', function(bool)
                        if bool then
                            isfling = true
                            pos = speaker.Character.HumanoidRootPart.Position
                            local flingTarget = target
                            function findAuraPlayer()
                                for i,v in pairs(game.Players:GetChildren()) do
                                    if v ~= speaker then
                                        if not CheckPlayerIsWhitelist(v) then
                                            if getRoot(v) and getRoot(speaker) then
                                                if v.Character ~= nil and v.Character.HumanoidRootPart ~= nil then
                                                    local mag = math.floor((v.Character.HumanoidRootPart.Position - pos).Magnitude)
        
                                                    if mag < FlingAura then
                                                        return v
                                                    end
                                                end
                                            end
                                        end
                                    end
                                end
                                return nil
                            end
                            spawn(function()
                                repeat
                                    flingTarget = findAuraPlayer()
                                    wait()
                                until isfling == false
                            end)
                            Root = getRoot(speaker)
                            noclip2(true)
                            AuraFling = game:GetService('RunService').Heartbeat:Connect(function()
    
                                local dis = 0.2
                                local increase = 6
                                local xchange
                                local zchange
                                local ychange
                                
                                local FlingEnemy
                                FlingEnemy = flingTarget.Character
                                if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                                    xchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                                    xchange = increase
                                else
                                    xchange = 0
                                end
                                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                    zchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                    zchange = increase
                                else
                                    zchange = 0
                                end  
                                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                    ychange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                    ychange = increase
                                else
                                    ychange = 0
                                end
                                if FlingEnemy ~= nil and flingTarget ~= nil then   
                                    Root.Velocity = Vector3.new(50000, 50000, 50000)
                                    Root.CFrame = CFrame.new(FlingEnemy.HumanoidRootPart.Position.X + math.random(-dis,dis) + xchange, FlingEnemy.HumanoidRootPart.Position.Y, FlingEnemy.HumanoidRootPart.Position.Z + math.random(-dis,dis) + zchange) * CFrame.Angles(math.random(0,100), math.random(0,100),math.random(0,100))
                                else
                                    Root.CFrame = CFrame.new(pos) * CFrame.Angles(math.random(0,100), math.random(0,100),math.random(0,100))
                                end
                            end)
                        else
                            isfling = false
                            AuraFling:Disconnect()
                            getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
                            getRoot(speaker).CFrame = CFrame.new(pos)
                            noclip2(false)
                            
                        end
                    end)
                    Freatures:CreateToggle('Jogar Player do mapa - R15', function(bool)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        fling = bool
                        
                        wait()
                        if fling == true then
                            noclip2(true)
                            oldpos = HRP.Position
                            
                            loopFling = game.RunService.Heartbeat:Connect(function()
                                local dis = 0.2
                                local increase = 6
                                local xchange
                                local zchange
                                local ychange
                                local FlingEnemy = target.Character
                                if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                                    xchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                                    xchange = increase
                                else
                                    xchange = 0
                                end
                                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                    zchange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                    zchange = increase
                                else
                                    zchange = 0
                                end  
                                if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                    ychange = -increase
                                elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                    ychange = increase
                                else
                                    ychange = 0
                                end
                                speaker.Character.HumanoidRootPart.Velocity = Vector3.new(50000, 50000, 50000)
                                speaker.Character.HumanoidRootPart.CFrame = CFrame.new(target.Character.UpperTorso.Position.X + math.random(-dis, dis) + xchange, target.Character.UpperTorso.Position.Y , target.Character.UpperTorso.Position.Z + math.random(-dis, dis) + zchange) * CFrame.Angles(FlingEnemy.HumanoidRootPart.Orientation.X + math.random(0, 100), FlingEnemy.HumanoidRootPart.Orientation.Y + math.random(0, 100), FlingEnemy.HumanoidRootPart.Orientation.Z + math.random(0, 100))
                            end)
                            repeat wait() until fling == false
                            HRP.Velocity = Vector3.new(0, 0, 0)
                            
                            loopFling:Disconnect()
                            game:GetService('TweenService'):Create(HRP, TweenInfo.new(0.2), {CFrame = CFrame.new(oldpos)}):Play()
                            noclip2(false) 
                        end
                    end)
                    
                    Freatures:CreateButton('Explodir player', function()
                        local explode = Instance.new('Explosion', workspace)

                        explode.Position = target.Character.Head.Position
                    end)
                    Freatures:CreateWhile('Explodir player (loop) NEW', function()
                        local explode = Instance.new('Explosion', workspace)

                        explode.Position = target.Character.Head.Position
                    end)
                    Freatures:CreateToggle('Jogar Player do mapa - R6', function(bool)
                        HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                        flingr6 = bool
                        
                        wait()
                        if flingr6 == true then
                            noclip2(true)
