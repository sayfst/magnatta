                    function AddFindPlayer(Freatures, text)
                        Freatures:CreateTextBox(text,'String', function(bool)
                            target = FindPlayer(bool)
                            getgenv().targetSelect = target.Name
                            if target == nil then
                                Notify(
                                    'Error to find '..bool,
                                    'Please try again',
                                    getPlayerImage(speaker),
                                    1
                                )
                            else
                                Notify(
                                    'Magnatta menu',
                                    'Selected:\n'..target.Name,
                                    getPlayerImage(target),
                                    3
                                )
                            end
                        end)
                    end
