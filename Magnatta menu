                                function Items:CreateToggle(title, callback, autoSave)
                                    local fName = 'Magnatta Toggless' 
                                    if not isfolder(fName) then
                                        makefolder(fName)
                                    end
                                    autoSave = autoSave or false
                                    


                                    callback = callback or function()end
                                    local Toggled = false
                        
                                    
                                    local Toggle = Instance.new("TextLabel")
                                    local ToggleBotao = Instance.new("TextButton")
                                    ToggleBotao.Active = false
                                    local TituloToggle = Instance.new("TextButton")
                                    TituloToggle.Active = false
                                    local function ative(forced)
                                        if not CanUse then return end
                                        warn = false
                                        if forced == 'toggled' then
                                            warn = false
                                        else
                                            warn = true
                                        end
                                        Toggled = not Toggled
                                        if Toggled then
                                            writefile(fName..'/'..title..'.lua', game:GetService('HttpService'):JSONEncode(Toggled))
                                        else
                                            if isfile(fName..'/'..title..'.lua') then
                                                delfile(fName..'/'..title..'.lua')
                                            end
                                        end
                                        spawn(function()
                                            if Toggled == true then
                                                ToggleBotao.BackgroundColor3 = Color3.fromRGB(239, 199, 0)
                                            else
                                                ToggleBotao.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
                                            end
                                        end)
                                        pcall(callback, Toggled, ative, warn)
                                    end

                                    if autoSave and isfile(fName..'/'..title..'.lua') then
                                        local IsAuto = game:GetService('HttpService'):JSONDecode(readfile(fName..'/'..title..'.lua'))
                                        if IsAuto == nil then IsAuto = false end
                                        if IsAuto == true then
                                            ative() 
                                        end
                                    end

                                    getgenv().Toggle[#getgenv().Toggle + 1] = {
                                        tt = title,
                                        ff = ative,
                                    }

                                    ToggleBotao.MouseButton1Click:Connect(function()
                                        ative('toggled')
                                    end)
                                    TituloToggle.MouseButton1Click:Connect(function()
                                        ative('toggled')
                                    end)
                        
                                    Toggle.Name = "Toggle"
                                    Toggle.Parent = TabLocal
                                    Toggle.Active = true
                                    Toggle.AnchorPoint = Vector2.new(0.5, 0.5)
                                    Toggle.BackgroundColor3 = config[2]
                                    Toggle.BorderSizePixel = 0
                                    Toggle.Position = UDim2.new(0.0122783082, 0, 0.132352948, 0)
                                    Toggle.Selectable = true
                                    Toggle.Size = UDim2.new(0, 18, 0, 18)
                                    Toggle.Font = Enum.Font.SourceSans
                                    Toggle.Text = ""
                                    Toggle.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    Toggle.TextScaled = true
                                    Toggle.TextSize = 14.000
                                    Toggle.TextWrapped = true
                                    Toggle.TextXAlignment = Enum.TextXAlignment.Left
                                    
                                    ToggleBotao.Name = "ToggleBotao"
                                    ToggleBotao.Parent = Toggle
                                    ToggleBotao.AnchorPoint = Vector2.new(0.5, 0.5)
                                    ToggleBotao.BackgroundColor3 = Color3.fromRGB(33,33,33)
                                    ToggleBotao.BorderSizePixel = 0
                                    ToggleBotao.Position = UDim2.new(0.5, 0, 0.5, 0)
                                    ToggleBotao.Size = UDim2.new(0, 15, 0, 15)
                                    ToggleBotao.Font = Enum.Font.SourceSans
                                    ToggleBotao.Text = ""
                                    ToggleBotao.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    ToggleBotao.TextScaled = true
                                    ToggleBotao.TextSize = 14.000
                                    ToggleBotao.TextWrapped = true
                                    ToggleBotao.TextXAlignment = Enum.TextXAlignment.Left
                                    
                                    
                                    TituloToggle.Name = "TituloToggle"
                                    TituloToggle.Parent = ToggleBotao
                                    TituloToggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    TituloToggle.BackgroundTransparency = 1.000
                                    TituloToggle.Position = UDim2.new(1.06666672, 0, -0.466666698, 0)
                                    TituloToggle.Size = UDim2.new(0, 683, 0, 26)
                                    TituloToggle.Font = Enum.Font.SourceSans
                                    TituloToggle.Text = title
                                    TituloToggle.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TituloToggle.TextScaled = true
                                    TituloToggle.TextSize = 14.000
