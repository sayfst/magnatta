                    local speaker = game.Players.LocalPlayer
                    Player:CreateToggle('Anti kill', function(bool)
                        getgenv().ak = bool
                    
                        if bool then
                            local canSave = true
                            local OLDPOS = nil
                            local AntiKill = game.RunService.Heartbeat:Connect(function()
                                if speaker.Character.Humanoid.Health < 20 then
                                    
                                    if canSave == true then
                                        OLDPOS = speaker.Character.HumanoidRootPart.CFrame
                                        canSave = false
                                    end
                                    speaker.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                                    speaker.Character.HumanoidRootPart.CFrame = CFrame.new(9999, 999999, 999999)
                                else
                                    if OLDPOS ~= nil then
                                        canSave = true
                                        speaker.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
                                        speaker.Character.HumanoidRootPart.CFrame = OLDPOS
                    
                                        OLDPOS = nil
                                    end
                                    
                                end
                            end)
                            repeat wait() until getgenv().ak == false
                            AntiKill:Disconnect()
                        end
                    end)
                    pcall(function()
                        Player:CreateToggle('Auto refresh', function(bool)
                            getgenv().ak = bool
                        
                            if bool then
                                local canSave = true
                                local OLDPOS = nil
                                local AntiKill = game.RunService.Heartbeat:Connect(function()
                                    if speaker.Character.Humanoid.Health == 0 then
                                        RefreshPrisonlife()
                                        if canSave == true then
                                            OLDPOS = speaker.Character.HumanoidRootPart.CFrame
                                            canSave = false
                                        else
                        
                                        end                                    
                                    else
                                        if OLDPOS ~= nil then
                                            canSave = true
                                            
                                            speaker.Character.HumanoidRootPart.CFrame = OLDPOS
                                            wait()
                                            speaker.Character.HumanoidRootPart.CFrame = OLDPOS
                                            wait()
                                            speaker.Character.HumanoidRootPart.CFrame = OLDPOS
                                            wait()
                                            speaker.Character.HumanoidRootPart.CFrame = OLDPOS
                                            wait()
                                            
                                            OLDPOS = nil
                                        end
                                        
                                    end
                                end)
                                repeat wait() until getgenv().ak == false
                                AntiKill:Disconnect()
                            end
                        end)
                    end)
                    
                    function ChangeButton(button, Text1, Text2, Text1Function, Text2Function)
                        Text1 = Text1 or 'Custom1'
                        Text2 = Text2 or 'Custom2'
                        Text1Function = Text1Function or function()end
                        Text2Function = Text2Function or function()end
                        
                    
                        if button.Text == Text1 then
                            button.Text = Text2
                            Text1Function()
                        elseif button.Text == Text2 then
                            button.Text = Text1
                            Text2Function()
                        end
                    end
                    
                    Player:CreateButton('FE - God mode', function()
                        local speaker = game.Players.LocalPlayer
                        local Cam = workspace.CurrentCamera
                        local Pos, Char = Cam.CFrame, speaker.Character
                        local Human = Char and Char.FindFirstChildWhichIsA(Char, "Humanoid")
                        local nHuman = Human.Clone(Human)
                        nHuman.Parent, speaker.Character = Char, nil
                        nHuman.SetStateEnabled(nHuman, 15, false)
                        nHuman.SetStateEnabled(nHuman, 1, false)
                        nHuman.SetStateEnabled(nHuman, 0, false)
                        nHuman.BreakJointsOnDeath, Human = true, Human.Destroy(Human)
                        speaker.Character, Cam.CameraSubject, Cam.CFrame = Char, nHuman, wait() and Pos
                        nHuman.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
                        local Script = Char.FindFirstChild(Char, "Animate")
                        if Script then
                            Script.Disabled = true
                            wait()
                            Script.Disabled = false
                        end
                        nHuman.Health = nHuman.MaxHealth
                    end)
                    Player:CreateSlider('Gravity',0,196.2,function(vaalue)
                        workspace.Gravity = vaalue
                    end)
                    Player:CreateToggle('Swim', function(bool)
                        if bool then
                            workspace.Gravity = 0
                        else
                            workspace.Gravity = 196.2
                        end
    
                        local Humanoid = game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
                        local state = not bool
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Climbing,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.FallingDown,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Flying,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Freefall,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.GettingUp,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Jumping,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Landed,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Physics,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.PlatformStanding,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Ragdoll,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Running,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Seated,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics,state)
                        Humanoid:SetStateEnabled(Enum.HumanoidStateType.Swimming,state)
    
                        if bool then
                            Humanoid:ChangeState(Enum.HumanoidStateType.Swimming)
                        else
                            Humanoid:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
                        end
                    end)
                    
