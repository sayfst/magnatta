                            local Tabss = {}
                            local Posicao_Menus = -1500
                            function Tabss:CreateTab(TabTitle)
                                local Tab = Instance.new("Frame")
                                local titulo = Instance.new("TextLabel")
                                local Preto = Instance.new("Frame")
                                local Items = Instance.new("ScrollingFrame")
                                local UIGridLayout = Instance.new("UIGridLayout")
                        
                                
                        
                                local TABBUTTON = Instance.new("TextButton")
                                local background = Instance.new("Frame")
                        
                                Tab.Name = TabTitle
                                Tab.Parent = Principal
                                Tab.BackgroundColor3 = Color3.fromRGB(16, 134, 244)
                                Tab.BorderSizePixel = 0
                                Tab.Position = UDim2.new(-2.06722689, Posicao_Menus, -4.51999998, 50)
                                local OLD = Posicao_Menus
