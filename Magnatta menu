                                if CheckPlayerIsWhitelist(target) == false then
                                    HRP.Velocity = Vector3.new(50000, 50000, 50000)
                                    local dis = 3.85
                                    local increase = 6
                                    local xchange
                                    local zchange
                                    local ychange
                                    local FlingEnemy = target
                                    if getHumanoid(target).MoveDirection.X < 0 then
                                        xchange = -increase
                                    elseif getHumanoid(target).MoveDirection.X > 0  then
                                        xchange = increase
                                    else
                                        xchange = 0
                                    end
                                    if getHumanoid(target).MoveDirection.Z < 0 then
                                        zchange = -increase
                                    elseif getHumanoid(target).MoveDirection.Z > 0 then
                                        zchange = increase
                                    else
                                        zchange = 0
                                    end  
                                    if getHumanoid(target).MoveDirection.Z < 0 then
                                        ychange = -increase
                                    elseif getHumanoid(target).MoveDirection.Z > 0 then
                                        ychange = increase
                                    else
                                        ychange = 0
                                    end  
                                    
                                    speaker.Character.HumanoidRootPart.Velocity = Vector3.new(50000, 50000, 50000)
                                    speaker.Character.HumanoidRootPart.CFrame = CFrame.new(getHead(target).Position.X + math.random(-dis, dis) + xchange, getHead(target).Position.Y , getHead(target).Position.Z + math.random(-dis, dis) + zchange) * CFrame.Angles(getHead(target).Orientation.X + math.random(0, 100), getHead(target).Orientation.Y + math.random(0, 100), getHead(target).Orientation.Z + math.random(0, 100))
                                
                                end
                            end)
                            repeat wait() until flingr6 == false
                            HRP.Velocity = Vector3.new(0, 0, 0)
                            
                            loopFling:Disconnect()
                            game:GetService('TweenService'):Create(HRP, TweenInfo.new(0.2), {CFrame = CFrame.new(oldpos)}):Play()
                            noclip2(false) 
                        end
                    end)
                    
                    Freatures:CreateToggle('Cavalinho no player', function(bool)
                        getgenv().HeadSit = bool
                    
                        if bool then
                            local headsit = game:GetService('RunService').Heartbeat:Connect(function()
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = target.Character.Head.CFrame * CFrame.new(0, 0, 0.8)
                                game.Players.LocalPlayer.Character.Humanoid.Sit = true
                                game.Players.LocalPlayer.Character.HumanoidRootPart.Velocity = Vector3.new(0, 0, 0)
