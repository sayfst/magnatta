                            Frame4.BackgroundTransparency = 0.5
                            LocalBotoesAbrirTab.Name = "LocalBotoesAbrirTab"
                            LocalBotoesAbrirTab.Parent = Frame4
                            LocalBotoesAbrirTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                            LocalBotoesAbrirTab.BackgroundTransparency = 1.000
                            LocalBotoesAbrirTab.Size = UDim2.new(0, 768, 0, 22)
                            
                            UIGridLayout.Parent = LocalBotoesAbrirTab
                            UIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
                            UIGridLayout.CellSize = UDim2.new(0, 72, 0, 25)
                            UIGridLayout.CellPadding = UDim2.new(0, 0, 0, 0)
                            local Tabs = {}
                            Yellow = Color3.fromRGB(255, 0, 0)

                            local HowMuchTabs = 0
                            local max = 76
                            local TabOpened = nil
                            function Tabs:CreateTab(TabTittle, CustomColor)
                                
                                HowMuchTabs = HowMuchTabs + 1
                                UIGridLayout.CellSize = UDim2.new(0.1, 0, 0, 25)
                                CustomColor = CustomColor or config[2]
                                local Tab = Instance.new("Frame")
    
                                local TextButton = Instance.new("TextButton")
                                local IntValue = Instance.new('Color3Value', TextButton)
                                IntValue.Name = 'NormalColor'
                                IntValue.Value = CustomColor
                                function RemColor(a)
                                    spawn(function()
                                        for i,v in pairs(LocalBotoesAbrirTab:GetChildren()) do
                                        
                                            if v:IsA('TextButton') and v ~= a then
                                                game:GetService('TweenService'):Create(v, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), {TextColor3 = v['NormalColor'].Value}):Play()
                                            end
                                            task.wait(.1)
        
                                        end
                                    end)
                                    game:GetService('TweenService'):Create(a, TweenInfo.new(1, Enum.EasingStyle.Quart, Enum.EasingDirection.InOut), {TextColor3 = Yellow}):Play()
                                end
    
                                local TabLocal = Instance.new("ScrollingFrame")
                                TabLocal.AutomaticCanvasSize = Enum.AutomaticSize.Y
                                local UIListLayout = Instance.new("UIListLayout")
                                UIListLayout.Parent = TabLocal
                                UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
                                UIListLayout.Padding = UDim.new(0, 5)
                                Tab.Visible = false
                                TextButton.Parent = LocalBotoesAbrirTab
                                TextButton.BackgroundColor3 = Color3.fromRGB(47, 47, 47)
                                TextButton.BorderSizePixel = 0
                                TextButton.Size = UDim2.new(0, 68, 0, 17)
                                TextButton.Font = Enum.Font.SourceSans
                                TextButton.TextColor3 = CustomColor
                                TextButton.TextSize = 14.000
                                TextButton.Text = TabTittle
                                if #TabTittle > 8 then
                                    TextButton.TextSize = 10.000
                                end
                                TextButton.MouseButton1Click:Connect(function()
                                    if not CanUse then return end
                                    RemColor(TextButton)
                                    if TabOpened ~= TabTittle then
                                        TabOpened = TabTittle
                                        for i,v in pairs(Frame4:GetChildren()) do
                                            if v.Name ~= 'LocalBotoesAbrirTab' then
                                                v.Visible = false
                                            end
                                        end
                                        Tab.Visible = true
                                    else
                                        for i,v in pairs(Frame4:GetChildren()) do
                                            if v.Name ~= 'LocalBotoesAbrirTab' then
                                                v.Visible = false
                                            end
                                        end
                                    end
                                end)
                                
                                
                                Tab.Name = "Tab123 "..TabTittle
                                Tab.Parent = Frame4
                                Tab.BackgroundColor3 = Color3.fromRGB(42, 42, 42)
                                Tab.BorderSizePixel = 0
                                Tab.Position = UDim2.new(0.01171875, 0, 0.0918918923, 0)
                                Tab.Size = UDim2.new(0, 748, 0, 323)
                                
                                TabLocal.Name = "TabLocal"
                                TabLocal.Parent = Tab
                                TabLocal.BackgroundColor3 = Color3.fromRGB(36, 36, 36)
                                TabLocal.BorderSizePixel = 0
                                TabLocal.Position = UDim2.new(0.0106951874, 0, 0.0247678012, 0)
                                TabLocal.Selectable = false
                                TabLocal.Size = UDim2.new(0, 733, 0, 306)
                                TabLocal.ScrollBarThickness = 2
                                
                                
                        
                                local Items = {}
                        
                                function Items:CreateButton(title, callback, CustomColor)
                                    getgenv().ExecCmd[#getgenv().ExecCmd + 1] = {
                                        tt = title,
                                        ff = callback
                                    }
                                    CustomColor = CustomColor or config[2]
                                    callback = callback or function()end
                                    local Botao = Instance.new("TextButton")
                                    
                                    local Bindable = false
    
    
                                    local KeyBind = Enum.KeyCode.LeftControl
                                    Botao.Name = "Botao"
                                    Botao.Parent = TabLocal
                                    Botao.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    Botao.BackgroundTransparency = 1.000
                                    Botao.BorderSizePixel = 0
                                    Botao.Size = UDim2.new(0, 701, 0, 26)
                                    Botao.Font = Enum.Font.SourceSans
                                    Botao.Text = title
                                    Botao.TextColor3 = CustomColor
                                    Botao.TextScaled = true
                                    Botao.TextSize = 14.000
                                    Botao.TextWrapped = true
                                    Botao.Active = false
                                    Botao.TextXAlignment = Enum.TextXAlignment.Left
                                    Botao.MouseButton1Click:Connect(function()
                                        if not CanUse then return end
                                        pcall(callback, Botao)
                                    end)
                                    Botao.MouseButton2Click:Connect(function()
                                        if Bindable == false then
                                            Notify(
                                                'Bind Function',
                                                'Press Any key...',
                                                getImage('Bell'),
                                                4
                                            )
                                            bind = game:GetService('UserInputService').InputBegan:Connect(function(bool)
                                                KeyBind = bool.KeyCode
                                                Bindable = true
                                                bind:Disconnect()
                                                Notify(
                                                'Bind Function',
                                                'Sucess bind',
                                                getImage('Bell'),
                                                4
                                            )
                                                Bindando = game:GetService('UserInputService').InputBegan:Connect(function(a,b)
                                                    if b then return end
    
                                                    if a.KeyCode == KeyBind then
                                                        pcall(callback)
                                                    end
                                                end)
                                                spawn(function()
                                                    repeat wait() until Bindable == false
                                                    Bindando:Disconnect()
                                                end)
                                            end)
                                            
                                        else
                                            Bindable = false
    
                                            Notify(
                                                'UnBind Function',
                                                'Suces unbind function',
                                                getImage('Bell'),
                                                4
                                            )
                                        end
                                    end)
                                end
                                function Items:CreateTextLabel(title)
                                    local TextBox = Instance.new("TextLabel")
                        
                                    TextBox.Parent = TabLocal
                                    TextBox.BackgroundColor3 = config[3]
                                    TextBox.BackgroundTransparency = 0.5
                                    TextBox.BorderSizePixel = 0
                                    TextBox.Size = UDim2.new(0, 701, 0, 26)
                                    TextBox.Font = Enum.Font.SourceSans
                                    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TextBox.TextScaled = true
                                    TextBox.TextSize = 14.000
                                    TextBox.TextWrapped = true
                                    TextBox.Text = title
                                end
                                function Items:CreateTextBox(title,valor,callback)
                                    callback = callback or function()end
                                    title = title .. ' ' .. config[4] or ''
                                    valor = valor or 'String'
                        
                                    local TextBox = Instance.new("TextBox")
                        
                                    TextBox.Parent = TabLocal
                                    TextBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                                    TextBox.BackgroundTransparency = 1.000
                                    TextBox.BorderSizePixel = 0
                                    TextBox.Size = UDim2.new(0, 701, 0, 26)
                                    TextBox.Font = Enum.Font.SourceSans
                                    TextBox.PlaceholderText = title.. ""
                                    TextBox.TextColor3 = Color3.fromRGB(255, 255, 255)
                                    TextBox.TextScaled = true
                                    TextBox.TextSize = 14.000
                                    TextBox.TextWrapped = true
                                    TextBox.Text = ''
                                    TextBox.TextXAlignment = Enum.TextXAlignment.Left
                                    TextBox.FocusLost:Connect(function(a)
                                        if not CanUse then return end
                                        if a then
                                            if valor == 'String' then
                                                pcall(callback, TextBox.Text)
                                            elseif valor == 'Number' then
                                                if TextBox.Text == 'inf' then
                                                    pcall(callback, math.huge)
                                                else
                                                    pcall(callback, tonumber(TextBox.Text))
                                                end
                                            end
                                        end
                                        task.wait()
                                        TextBox.Text = ''
                                        TextBox.PlaceholderText = title
                                    end)
                                end                                     
