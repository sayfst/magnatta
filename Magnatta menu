                        --[[Build a boat For Treasure]] if game.PlaceId == 537413528 then
                            ChangeName('Build a boat')
                            local AutoFarmVelocity = 100
                            local isOnAutoFarm = false
                            function AutoFarm()
                                isOnAutoFarm=true;
                                if getRoot(speaker) and getHumanoid(speaker).Health > 0 then
                                    getRoot(speaker).Velocity = Vector3.new(0, 0, 0)
                                    teleport(CFrame.new(-49.024314880371094, 80.19031524658203, 1201.065673828125))
                                    task.wait()
    
                                    local FinishPos = Vector3.new(-50, 68, 8400)
                                    
                                    local bodyVelocity = Instance.new('BodyPosition', getRoot(speaker))
                                    bodyVelocity.MaxForce = Vector3.new(math.huge,math.huge,math.huge)
                                    bodyVelocity.D = 500
                                    bodyVelocity.P = AutoFarmVelocity
                                    bodyVelocity.Position = FinishPos
                                    repeat
                                        bodyVelocity.P = AutoFarmVelocity
                                        task.wait()
                                    until (getRoot(speaker).Position - FinishPos).Magnitude <= 40 or getHumanoid(speaker).Health == 0
                                    local canContinue = false;
                                    if getHumanoid(speaker).Health == 0 then
                                        Notify('', 'Died', getImage('Bell'), 5)
                                        canContinue = false
                                        bodyVelocity:Destroy()
                                    else
                                        canContinue = true
                                    end
                                    bodyVelocity:Destroy()
                                    if canContinue == true then
                                        repeat
                                            teleport(CFrame.new(-55.69078826904297, -359.29248046875, 9494.26953125))
                                            task.wait()
                                        until speaker.CharacterAdded:wait()
                                    end
                                    wait(1)
                                end
                                isOnAutoFarm=false;
                            end
    
                            SupportGame:CreateTextLabel('Auto farm')
    
                            SupportGame:CreateSlider('Auto farm - Velocity', 20, 3000, function(b)
                                AutoFarmVelocity=b;
                            end)
                            SupportGame:CreateButton('Auto farm - Once', function()
                                AutoFarm()
                            end, corlegal)
                            SupportGame:CreateToggle('Auto farm - On/Off', function(bb)
                                getgenv().AutoFarm = bb
                                spawn(function()
                                    while getgenv().AutoFarm == true do
                                        if isOnAutoFarm == false then
                                            AutoFarm()
                                        end
                                        wait()
                                    end
                                end)
                            end)
                            vez = 0
                            SupportGame:CreateButton('Auto farm - Cant stop', function()
                                vez = vez + 1
                                if vez == 1 then
                                    Notify(
                                        'Warn',
                                        'If you press it one more time, you will no longer be able to stop your auto farm, to stop, just re-entering the game',
                                        getImage('Bell'),
                                        15)
                                elseif vez == 2 or vez >=2 then
                                    game:GetService('RunService').RenderStepped:Connect(function()
                                        if isOnAutoFarm == false then
                                            AutoFarm()
                                        end
                                    end)
                                end
                            end, corlegal)
                        end
