                            b:CreateToggle('Fling', function(bool)
                                HRP = game.Players.LocalPlayer.Character.HumanoidRootPart
                                fling = bool
                                
                                wait()
                                if fling == true then
                                    noclip2(true)
                                    oldpos = HRP.Position
                                    
                                    loopFling = game.RunService.Heartbeat:Connect(function()
                                        local dis = 0.2
                                        local increase = 6
                                        local xchange
                                        local zchange
                                        local ychange
                                        local FlingEnemy = target.Character
                                        if FlingEnemy.Humanoid.MoveDirection.X < 0 then
                                            xchange = -increase
                                        elseif FlingEnemy.Humanoid.MoveDirection.X > 0  then
                                            xchange = increase
                                        else
                                            xchange = 0
                                        end
                                        if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                            zchange = -increase
                                        elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                            zchange = increase
                                        else
                                            zchange = 0
                                        end  
                                        if FlingEnemy.Humanoid.MoveDirection.Z < 0 then
                                            ychange = -increase
                                        elseif FlingEnemy.Humanoid.MoveDirection.Z > 0 then
                                            ychange = increase
                                        else
                                            ychange = 0
                                        end
                                        speaker.Character.HumanoidRootPart.Velocity = Vector3.new(50000, 50000, 50000)
                                        speaker.Character.HumanoidRootPart.CFrame = CFrame.new(target.Character.UpperTorso.Position.X + math.random(-dis, dis) + xchange, target.Character.UpperTorso.Position.Y , target.Character.UpperTorso.Position.Z + math.random(-dis, dis) + zchange) * CFrame.Angles(FlingEnemy.HumanoidRootPart.Orientation.X + math.random(0, 100), FlingEnemy.HumanoidRootPart.Orientation.Y + math.random(0, 100), FlingEnemy.HumanoidRootPart.Orientation.Z + math.random(0, 100))
                                    end)
                                    repeat wait() until fling == false
                                    HRP.Velocity = Vector3.new(0, 0, 0)
                                    
                                    loopFling:Disconnect()
                                    game:GetService('TweenService'):Create(HRP, TweenInfo.new(0.2), {CFrame = CFrame.new(oldpos)}):Play()
                                    noclip2(false) 
                                end
                            end)
                            b:CreateToggle('View', function(bool)
                                if bool then
                                    workspace.CurrentCamera.CameraSubject = getCharacter(target)
                                else
                                    workspace.CurrentCamera.CameraSubject = getCharacter(speaker)
                                end
                            end)
