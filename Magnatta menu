                    local multiList = {}
                    function CheckPlayerIsWhitelist(plr)
                        for i = 1, #multiList do
                            if multiList[i] == plr.Name then
                                return true
                            end
                        end
                        return false
                    end
                    WhitelistTab:CreateTextLabel('Here you can put friends, Or people you want to get rid of wars, Kill all these things')
    
                    WhitelistTab:CreateTextLabel('Configure:')
    
                    WhitelistTab:CreateButton('Save whitelist Players', function()
                        writefile('Whitelist players.lua', game:GetService('HttpService'):JSONEncode(multiList))
                    end)
                    WhitelistTab:CreateButton('Load whitelist Players', function()
                        if isfile('Whitelist players.lua') then
                            local sp = game:GetService('HttpService'):JSONDecode(readfile('Whitelist players.lua'))
    
                            multiList = sp
                        else
                            Notify(
                                'Error to load',
                                'Whitelist players.lua',
                                getImage('Bell'),
                                5
                            )
                        end
                    end)
                    WhitelistTab:CreateButton('Print Whitelisteds', function()
                        print('\nPlayers whitelisted:\n')
                        for i,v in pairs(multiList) do
                            print(v)
                        end
                    end)
                        
                    function addOnWhitelist(plr)
                        multiList[#multiList + 1] = plr.Name
                    end
                    function remOnWhitelist(plr)
                        lugar = 0
                        for i = 1,#multiList do
                            if multiList[i] == plr.Name then
                                table.remove(multiList, i)
                            end 
                        end
    
                    end
                    
                    WhitelistTab:CreateTextLabel('Players:')
    
                    function CreatePlayerWhitelist(plr)
                        WhitelistTab:CreateToggle(plr.Name, function(bool)
                            if bool then
                                addOnWhitelist(plr)
                            else
                                remOnWhitelist(plr)
                            end
                        end)
                    end
                    for i,v in pairs(game.Players:GetChildren()) do
                        if v ~= speaker then
                            CreatePlayerWhitelist(v)
                        end
                    end
                    game.Players.ChildAdded:Connect(function(plr)
                        CreatePlayerWhitelist(plr)
                    end)
                    game.Players.ChildRemoved:Connect(function(plr)
                        for i,v in pairs(game:GetService("CoreGui")["Magnatta v2 2"].Frame.Frame1.Frame2.Frame3.Frame4["Tab123 Lista branca"].TabLocal:GetDescendants()) do
                            if v:IsA('TextButton') then
                                if v.Text == plr.Name then
                                    remOnWhitelist(plr)
                                    if v.Parent.Name == 'ToggleBotao' then
                                        v.Parent.Parent:Destroy()
                                    end
                                end
                            end
                        end
                    end)
    
                    hNeed = ' {Hats needed}'
                    noHneed = ' {No Hats neeeded}'
    
                    function copyHats(hats)
                        local games = hats
                        text = ''
                        num = 0
                        for i,v in pairs(games) do
                            if v then
                                num = num + 1
                                text = text .. '\nHat '..num..': '..v
                            end
                        end
                        setclipboard(text)
                    end
                    function R15(speaker)
                        if speaker.Character.Humanoid.RigType == Enum.HumanoidRigType.R15 then
                            return true;
                        else
                            return false;
                        end
                    end

