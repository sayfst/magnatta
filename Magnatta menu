                    local Tab_3Created = {}

                    function AddOnTab(oque)
                        Tab_3Created[#Tab_3Created + 1] = oque
                    end
                    Etc:CreateTextBox('Add item', 'String', function(arg)
                        if arg == 'lplrPos' then
                            local pos = speaker.Character.HumanoidRootPart.CFrame
                            AddOnTab(CFrame.new(tostring(pos)))
                        elseif arg == '{}' then
                            AddOnTab({})
                        elseif arg == 'String' then
                            AddOnTab('"'..string.gsub(arg, 'String ', '')..'"')
                        elseif arg == 'Number' then
                            AddOnTab(tonumber(string.gsub(arg, 'Number ', '')))
                        elseif arg == 'lplrtColor' then
                            AddOnTab('"'..speaker.TeamColor.Name..'"')
                        end
                    end)


                    Etc:CreateButton('Clear tab', function()
                        Tab_3Created = {}
                    end)
                    Etc:CreateButton('undo last argument', function()
                        table.remove(Tab_3Created, #Tab_3Created)
                    end)
                    Etc:CreateButton('Copy Tab', function()
                        setclipboard(Tab_3Created)
                    end)

